  "version": "0.2.0",
  "configurations": [
    {
      "name": "Engine: Debug EXE (VS Debugger)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\build\\Debug\\Prey2006.exe",
      "cwd": "${workspaceFolder}\\output\\windows",
      "args": ["+set", "developer", "1"],
    },
    {
      "name": "Engine: Debug Optimized EXE (VS Debugger)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\build\\RelWithDebInfo\\Prey2006.exe",
      "cwd": "${workspaceFolder}\\output\\windows",
      "args": ["+set", "developer", "1"],
    },
  ],
}