//		alphatest 0.5
	}
	specularmap		textures/roadhouse/arcade_buttons_s


	//global alphatest stages
	{
		if ( parm5 == 0 )
		blend none
		map textures/roadhouse/arcade_buttons_d
		alphaTest 0.5
	}
	{
		if ( parm5 > 0 ) //fade out
		blend none
		map textures/sfx/roadhouse_ramp_fx_256
		alphaTest ( time + parm4 ) * global6
	}

	//additive fx stage
	{
		if ( parm5 > 0 ) //fade out
		privatepolygonoffset	-1

		blend add
		map textures/sfx/roadhouse_ramp_fx_256
		alphaTest ( time + parm4 ) * global6 - global7
		rgb 2
		glowStage
	}
}







