textures/roadhouse/faucetwater
{
	translucent
	noshadows
	nonsolid
	noimpact
	{
		program			heathaze.vfp

		vertexparm		0       0.0 , time * 5	// texture scrolling
		vertexparm		1		parm5
		fragmentprogram			heathaze.vfp
		fragmentmap		0       _currentrender
		fragmentmap		1       textures/particles/heat1	// the normal map for distortion
	}
	{
		blend		diffusemap
		map			textures/roadhouse/faucetwater_d
		colored
		scroll		0.0, time * 5
	}
}

textures/roadhouse/faucetbasin
{
	noshadows
	translucent
	forceoverlays

	qer_editorimage textures/roadhouse/faucetwater_d
	{
		program			heathaze.vfp

		vertexparm		0       time * 0.7 , time * 0.7 // texture scrolling
		vertexparm		1		parm5
		fragmentprogram			heathaze.vfp
		fragmentmap		0       _currentrender
		fragmentmap		1       textures/roadhouse/faucetbasin	// the normal map for distortion
	}
	{
		blend		diffusemap
		map			textures/roadhouse/faucetwater_d
		colored
		//scroll		0.0, time * 5
	}
}

textures/roadhouse/toiletwater2
{
	noshadows
	translucent
	forceoverlays

	qer_editorimage textures/roadhouse/faucetwater_d
