textures/roadhouse/barside1_dissolve
{
	qer_editorimage 	textures/roadhouse/barside1_d
	matter_wood
	bumpmap			addnormals( textures/roadhouse/barside1_local, heightmap( textures/roadhouse/barside1_h, 4 ) )
	specularmap		textures/roadhouse/barside1_s
	diffusemap textures/roadhouse/barside1_d

	//global alphatest stage
	{
		if ( parm5 > 0 ) //fade out
		blend none
		map textures/sfx/roadhouse_ramp_fx_256
		alphaTest ( time + parm4 ) * global6
	}

	//additive fx stage
	{
		if ( parm5 > 0 ) //fade out
		privatepolygonoffset	-1
		blend add
		map textures/sfx/roadhouse_ramp_fx_256
		alphaTest ( time + parm4 ) * global6 - global7
		rgb 2
		glowStage
	}
}

