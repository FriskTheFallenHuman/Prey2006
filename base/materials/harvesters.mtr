



textures/harvesters/abdowall1
{
	qer_editorimage 	textures/harvesters/abdowall1_d
	bumpmap 		addnormals (textures/harvesters/abdowall1_local, heightmap (textures/harvesters/abdowall1_h, 6))
//	bumpmap 		textures/harvesters/abdowall1_local
	diffusemap	 	textures/harvesters/abdowall1_d
	specularmap		textures/harvesters/abdowall1_s

}


textures/harvesters/abdowall1b
