table magicdoor_fadeout { clamp { 0, 1 } }
table magicdoor_fadein { clamp { 1, 0 } }
table magicdoor_pulse { { 0, 1, 1, 1, 1 } }
table magicdoor_birdout { clamp { 1, 1, 1, 0 } }
table magicdoor_birdin { clamp { 0, 1, 0 } }
models/mapobjects/lota/arch/magic_bricks_arch
{
	matter_stone
	//noselfshadow
	noshadows
	twosided

	renderbump  -size 1024 1024 -trace 0.09 -aa 1   models/mapobjects/lota/arch/magic_bricks_arch_local models/mapobjects/lota/arch/magic_bricks_arch_hi.ase
	specularmap models/mapobjects/lota/arch/magic_bricks_arch_s
	diffusemap	models/mapobjects/lota/arch/magic_bricks_arch
	bumpmap		addnormals (models/mapobjects/lota/arch/magic_bricks_arch_local,
				heightmap  (models/mapobjects/lota/arch/magic_bricks_arch_h, 4 ) )

	{	if ( parm7 == 1 )
		maskcolor
		blend gl_zero, gl_one
		map textures/sfx/genericdissolve
		alphaTest 0.1 + magicdoor_fadeout[ (time + parm4) * 0.5 ]
	}
	{	if ( parm7 == 1 )
		privatePolygonOffset -1
		map textures/sfx/genericdissolve
		alphaTest magicdoor_fadeout[ (time + parm4) * 0.5 ]
		//color 0.29, 0.4, 0.5, 1
		color 0.59, 0.8, 1.0, 1
		glowStage
	}
	{	if ( parm7 == 2 )
		maskcolor
		blend gl_zero, gl_one
		map textures/sfx/genericdissolve
		//alphaTest 0.1 + magicdoor_fadein[ (time + parm4) * 0.5]
		alphaTest magicdoor_fadein[ (time + parm4) * 0.6] * 0.3 + magicdoor_fadein[ (time + parm4) * 0.6]
	}
	{	if ( parm7 == 2 )
		privatePolygonOffset -1
		map textures/sfx/genericdissolve
		alphaTest magicdoor_fadein[ (time + parm4) * 0.6]
		color 0.59, 0.8, 1.0, 1
		glowStage
	}
	{	if ( parm5 == 1 ) // pulse bird symbol
		blend   add
		map     models/mapobjects/lota/arch/magic_bricks_arch_add
		red     0.29 * 0.1 + (0.29 * magicdoor_pulse[time / 3]) * 0.4
		green   0.6  * 0.1 + (0.6  * magicdoor_pulse[time / 3]) * 0.4
		blue    0.9  * 0.1 + (0.9  * magicdoor_pulse[time / 3]) * 0.4
		glowStage
	}
	{	if ( parm5 == 2 ) // fade in bird symbol
		blend   add
		map     models/mapobjects/lota/arch/magic_bricks_arch_add
		//red     (0.29 * growin[ (time - parm6) / 1.5 ]) * 0.8
		//green   (0.6  * growin[ (time - parm6) / 1.5 ]) * 0.8
		//blue    (0.9  * growin[ (time - parm6) / 1.5 ]) * 0.8
		red     (0.29 * magicdoor_birdin[ (time - parm6) / 2.5 ]) * 0.8
		green   (0.6  * magicdoor_birdin[ (time - parm6) / 2.5 ]) * 0.8
		blue    (0.9  * magicdoor_birdin[ (time - parm6) / 2.5 ]) * 0.8
		glowStage
	}
	{	if ( parm5 == 3 ) // fade out bird symbol
		blend   add
		map     models/mapobjects/lota/arch/magic_bricks_arch_add
		red     (0.29 * magicdoor_birdout[ (time - parm6) / 3 ]) * 0.8
		green   (0.6  * magicdoor_birdout[ (time - parm6) / 3 ]) * 0.8
		blue    (0.9  * magicdoor_birdout[ (time - parm6) / 3 ]) * 0.8
		glowStage
	}
}

