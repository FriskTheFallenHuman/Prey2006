models/mapobjects/jennytank/tank
{
	matter_metal
	noselfshadow
	twosided
	translucent
//	deform jitter jacobladder[( time + parm7 ) * ( .5 + parm4 )]

	renderbump  -size 1024 1024 -trace 0.09 -aa 1   models/mapobjects/jennytank/tank_local models/mapobjects/jennytank/tank_hi.ase
//	specularmap models/mapobjects/jennytank/tank_s
	{
		blend 	specularmap
		map		models/mapobjects/jennytank/tank_burn
		rgb growin[ 0.45 * (time + parm4) ]
	}
//	diffusemap	models/mapobjects/jennytank/tank
	{
		blend 	diffusemap
		map		models/mapobjects/jennytank/tank_burn
		rgb growin[ 0.45 * (time + parm4) ]
	}
	{ // If the sequence has not started yet, then force the alpha test to off (which is 1 in this case)
		if parm4 == 0 && parm5 == 0
		blend	gl_zero, gl_one			// don't draw anything
		map models/mapobjects/jennytank/tank_burn
		alphaTest 1
	}
//	bumpmap		models/mapobjects/jennytank/tank_local
	{
		blend	add
		map 	models/mapobjects/jennytank/tank_burn
//		rgb 0.5
		rgb growin[ 0.45 * (time + parm4) ]
		glowStage
	}
	{
		blend add
		map 	models/mapobjects/jennytank/tank_ad_2
		rgb 	.3 * headlightpulse [time * 2.5]
		scroll  .25 * sintable[time * 2.9], 0
		glowStage
	}
	{
		blend add
		map 	models/mapobjects/jennytank/tank_ad_2
		rgb 	.3 * headlightpulse [time * 0.75]
		scroll  .25 * costable[time * 3.6], 0
		glowStage
	}
// 	burnout for tank destruction

/*	{	// Building effect
		if	parm4
		// make a burned away alpha test for the normal skin
		blend	gl_zero, gl_one			// don't draw anything
		map models/mapobjects/jennytank/tank_burn
		alphaTest burnout[ 0.25 * (time + parm4) ]
//		alphaTest 0.25 * (time + parm4)
	}*/

	/*
	{
		if	parm4
		// draw the burn at a negative polygonOffset, so it is behind the other stages
		privatePolygonOffset	-1		// stage-only polygon offset
        blend add
        blend gl_one, gl_zero

		map models/mapobjects/jennytank/tank_burn	// Model specific texture.
		alphaTest burnout[ 2 * (time + parm4) ] - 0.02
		glowStage
	}
*/

	{	// Disintegrating tank effect - burns away the diffuse map
		if	parm5
		// make a burned away alpha test for the normal skin
		blend	gl_zero, gl_one			// don't draw anything
		map models/mapobjects/jennytank/tank_burn	// Model specific texture. Only the alpha channel is referenced. Alphas from dark to light.
		alphaTest 0.01 + 0.25 * ((time + parm5))
	}

/*
	{	// burn effect - glowing effect that trails the burnout
		if	( parm4 != 0 )
		// draw the burn at a negative polygonOffset, so it is behind the other stages
		privatePolygonOffset	-1		// stage-only polygon offset
        blend add
        blend gl_one, gl_zero
		map models/mapobjects/jennytank/tank_burn	// Model specific texture.
		alphaTest 3.25 * (time + parm4 )
		glowStage
	}
*/
}

