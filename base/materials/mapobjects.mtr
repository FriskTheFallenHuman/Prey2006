models/mapobjects/sboss/st2
{
	qer_editorimage textures/vpaint/feeding2_proxy
	matter_flesh
	{
		shaderFallback2
		blend bumpmap
		map textures/vpaint/feeding2_local
		scale 0.25, 0.25
		scroll time * 0.5, time * 0.5
	}
	{
		shaderFallback2
		blend diffusemap
		map add( textures/vpaint/feeding2_d, textures/vpaint/feeding2_ss )
		scale 0.25, 0.25
		scroll time * 0.5, time * 0.5
	}
	{
		shaderFallback2
		blend specularmap
		map textures/vpaint/feeding2_s
		scale 0.25, 0.25
		scroll time * 0.5, time * 0.5
	}
	{
		shaderLevel2

		blend shader
		vertexprogram vertexscale.vp
		fragmentprogram deformInteraction.vfp

		vertexparm		0		0.25, 0.25, 0, 0 // scale x,y, scroll x,y

		fragmentmap		1		textures/vpaint/feeding2_local // normal
		fragmentmap		4		add( textures/vpaint/feeding2_d, textures/vpaint/feeding2_ss ) // diffuse
		fragmentmap		5		textures/vpaint/feeding2_s2 // specular
		fragmentmap		7		textures/glass/vp1 // deform texture

		fragmentparm	0		0.5, 0.5, time * 0.03, time * 0.03 // scale x,y, scroll x,y of deform
		fragmentparm	1		.02 // deform amount
		fragmentparm	2		300 // exponent
	}
	{
		if parm7
		blend gl_zero, gl_one
		map textures/sfx/genericdissolve
		alphaTest 0.05 + 0.35 * (time - parm4)
		centerscale 0.1, 0.1
	}
	{
		if parm7
		privatePolygonOffset -1
        blend add
		map textures/sfx/genericdissolve
		alphaTest 0.35 * (time - parm4)
		centerscale 0.1, 0.1
		glowStage
	}
	{
		if parm7
		privatePolygonOffset -1.3
		blend add
		map _white
		alphaTest 0
		glowStage
	}
}

