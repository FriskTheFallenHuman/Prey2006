lights/spherebreed/sb_caustic1_hanglight
// rotate parm5, scale parm6-7
{
	{
		map			lights/spherebreed/sb_caustic1_hanglight
		zeroclamp
		rotate		time * (.01 * parm5)
		colored
		centerscale	scale_1jitter[ time * parm6 ], scale_1jitter[ time * parm7 ]
	}
}

lights/spherebreed/sb_caustic1
// rotate parm5, scale parm6-7
{
	{
		map			lights/spherebreed/sb_caustic1
		zeroclamp
		rotate		time * (.01 * parm5)
		colored
		centerscale	scale_1jitter[ time * parm6 ], scale_1jitter[ time * parm7 ]
	}
}

textures/organic_trim/tornskin1_solid
{

//	next line is the only change between this and textures/organic_trim/tornskin1
//	nonsolid
	twosided
	matter_flesh
	qer_editorimage 	textures/organic_trim/tornskin1_d

	bumpmap 	textures/organic_trim/tornskin1_local
//	diffusemap	textures/organic_trim/tornskin1_d
	specularmap 	textures/organic_trim/tornskin1_s

	{
		blend diffusemap
		map	textures/organic_trim/tornskin1_d
		alphatest 0.7
	}

}
