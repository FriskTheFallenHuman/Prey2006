textures/organic_wall/digestive_tube1_shadowed
{
	twosided
//	noshadows
//	noselfshadow
//	translucent

	qer_editorimage 	textures/organic_wall/digestive_tube1_d

	bumpmap 		addnormals (textures/organic_wall/digestive_tube1_local, heightmap (textures/organic_wall/digestive_tube1_h, 6))
//	diffusemap	 	textures/organic_wall/digestive_tube1_d
	{
		blend diffusemap
		map	textures/organic_wall/digestive_tube1_d
		alphatest 0.4
	}
	{
		blend specularmap
		map	textures/organic_wall/digestive_tube1_s
		ignorealphatest
	}
	{

		blend blend
		map	textures/organic_wall/digestive_tube1_d3
		ignorealphatest
		alpha 0.5
	}
	{

		blend gl_zero, gl_one_minus_src_color
		map	textures/organic_wall/digestive_tube1_d2
		ignorealphatest
		alpha 0.7
	}


}

