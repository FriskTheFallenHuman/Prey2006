textures/dreamworld/la_darkrock_glow
{
	qer_editorimage		textures/dreamworld/la_darkrock_glow_d
	matter_stone


// White verts
	{
		blend	bumpmap
		map textures/dreamworld/la_darkrock_local
	}
	{
		blend diffusemap
		vertexcolor
		map		textures/dreamworld/la_darkrock_glow_d
	}
	{
		blend specularmap
		vertexcolor
		map		textures/dreamworld/la_darkrock_s
	}
	//add
	{

		blend gl_one_minus_dst_color, gl_one
		//blend gl_src_alpha, gl_one

		map textures/dreamworld/la_darkrock_glow_add
		glowStage


		vertexcolor



	}

// Black verts
	{
		blend	bumpmap
		map textures/dreamworld/la_darkrock_local
	}
	{
		blend diffusemap
		inversevertexcolor
		map		textures/dreamworld/la_darkrock_d
	}
	{
		blend specularmap
		inversevertexcolor
		map		textures/dreamworld/la_darkrock_s
	}
}
textures/dreamworld/la_darkrock_glow_disolve
{
	qer_editorimage		textures/dreamworld/la_darkrock_glow_d
	matter_stone


// White verts
	{
		blend	bumpmap
		map textures/dreamworld/la_darkrock_local
	}
	{
		blend diffusemap
		vertexcolor
		map		textures/dreamworld/la_darkrock_glow_d
	}
	{
		blend specularmap
		vertexcolor
		map		textures/dreamworld/la_darkrock_s
	}
	//add
	{

		blend gl_one_minus_dst_color, gl_one
		//blend gl_src_alpha, gl_one

		map textures/dreamworld/la_darkrock_glow_add2
		glowStage


		vertexcolor



	}

// Black verts
	{
		blend	bumpmap
		map textures/dreamworld/la_darkrock_local
	}
	{
		blend diffusemap
		inversevertexcolor
		map		textures/dreamworld/la_darkrock_d
	}
	{
		blend specularmap
		inversevertexcolor
		map		textures/dreamworld/la_darkrock_s
	}

// Burn in/out
	{
		if parm7
		blend gl_zero, gl_one
		map textures/sfx/genericdissolve
		alphaTest 0.05 + 0.35 * (time - parm4)
		centerscale 0.5, 0.5
	}
	{
		if parm7
		privatePolygonOffset -1
        blend add
		map textures/sfx/genericdissolve
		alphaTest 0.35 * (time - parm4)
		centerscale 0.5, 0.5
		glowStage
	}
	{
		if parm7
		privatePolygonOffset -1.3
		blend add
		map _white
		alphaTest 0
		glowStage
	}
}
