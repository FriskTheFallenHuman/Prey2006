textures/dreamworld/la_darkrockc
{
	qer_editorimage		textures/dreamworld/la_darkrock_d
	matter_stone
	bumpmap			textures/dreamworld/la_darkrock_local
	diffusemap		textures/dreamworld/la_darkrock_d
	specularmap		textures/dreamworld/la_darkrock_s
	{
		maskcolor
		map		makealpha( textures/dreamworld/la_darkrock_cm )
		scroll	time * 0.02, time * 0.03
		rotate	time * 0.003
	}
	{
		maskalpha
		map textures/dreamworld/la_darkrock_c
		blend	gl_dst_alpha, gl_one
		colored
	}
}
textures/dreamworld/la_darkrock_dis
{
	qer_editorimage		textures/dreamworld/la_darkrock_d
	matter_stone
	bumpmap			textures/dreamworld/la_darkrock_local
	diffusemap		textures/dreamworld/la_darkrock_d
	specularmap		textures/dreamworld/la_darkrock_s
	{
		if ( parm5 == 0 )
		blend none
		map textures/sfx/genericdissolve
		alphaTest 0
	}
	{
		if ( parm5 > 0 )
		blend none
		map textures/sfx/genericdissolve
		alphaTest ( time + parm4 ) * 0.5
	}
	{
		if ( parm5 > 0 )
		privatepolygonoffset	-1
		blend add
		map textures/sfx/genericdissolve
		alphaTest ( time + parm4 ) * 0.5 - 0.2
		//rgb 2
		red 0.68
		green 0.81
		blue 1.0
		glowStage
	}
}
