textures/tech_trim/hpipe3c
{
	noshadows
	matter_pipe
	// this stage will define the alphatest map, but not draw anything
	{
		blend add
		map	textures/tech_trim/hpipe3b_mask
		alphatest 0.50
	}
	{
		blend add
		map textures/tech_trim/hpipe3b_add
//		rgb 0.5
		color
		parm0 * heartbeat1[ time * 0.35 ],
		parm1 * heartbeat1[ time * 0.35 ],
		parm2 * heartbeat1[ time * 0.35 ],
		1
		scroll 0, time * 1
		scale 3,2 // 3, 0.125
		glowstage
	}
}

