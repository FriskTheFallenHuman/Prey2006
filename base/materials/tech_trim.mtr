textures/tech_trim/techtrim_057b2_clean_dissolve
{
	qer_editorimage 	textures/tech_trim/techtrim_057b2_clean_proxy
	matter_metal

	bumpmap 		textures/tech_trim/techtrim_057b2_clean_local

	diffusemap	 	textures/tech_trim/techtrim_057b2_clean_d
	specularmap		textures/tech_trim/techtrim_057b2_clean_s

	//global alphatest stages
	{
		if ( parm5 == 0 )
		blend none
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest 1 - ( time + parm4 ) * 0.5
	}
	{
		if ( parm5 > 0 ) //fade out
		blend none
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest ( time + parm4 ) * 0.5
	}

	//additive fx stage
	{
		if ( parm5 == 0 ) //fade out
		privatepolygonoffset	-1
		blend add
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest 1 - ( time + parm4 ) * 0.5 + 0.1
		rgb 2
		glowStage
	}
	{
		if ( parm5 > 0 ) //fade out
		privatepolygonoffset	-1
		blend add
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest ( time + parm4 ) * 0.5 - 0.1
		rgb 2
		glowStage
	}
}

