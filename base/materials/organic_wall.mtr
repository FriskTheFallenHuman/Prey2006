textures/organic_wall/bio_organic_400
{
	qer_editorimage 	textures/organic_wall/bio_organic_400_proxy
	matter_flesh
/*
	bumpmap			addnormals (textures/organic_wall/bio_organic_400_local,
				heightmap  (textures/organic_wall/bio_organic_400_h, 20))
	diffusemap		textures/organic_wall/bio_organic_400_d
//	specularmap		textures/organic_wall/bio_organic_400_s
	{
		blend specularmap
		map textures/organic_wall/bio_organic_400_s
		specularexp		22, 1.4
	}
*/
	{
		shaderFallback2
		blend 		bumpmap
		map		addnormals (textures/organic_wall/bio_organic_400_local,
				heightmap  (textures/organic_wall/bio_organic_400_h, 20))
		scale		parm4, parm5
		scroll		time * parm6, time * parm7
	}
	{
		shaderFallback2
		blend 		diffusemap
		map 		textures/organic_wall/bio_organic_400_d
		scale		parm4, parm5
		scroll		time * parm6, time * parm7
	}
	{
		shaderFallback2
		blend		specularmap
		map			textures/organic_wall/bio_organic_400_s
		scale		parm4, parm5
		scroll		time * parm6, time * parm7
	}
	{
		shaderLevel2

		blend shader
		vertexprogram vertexscale.vp
		fragmentprogram deformInteraction.vfp

		vertexparm		0		parm4, parm5, 0, 0		//scale x,y, scroll x,y

		fragmentmap		1		addnormals(textures/organic_wall/bio_organic_400_local,		// normal
								heightmap (textures/organic_wall/bio_organic_400_h, 20))
		fragmentmap		4		textures/organic_wall/bio_organic_400_d	// diffuse
		fragmentmap		5		textures/organic_wall/bio_organic_400_s							// specular
		fragmentmap		7		textures/glass/vp1			//deform texture

		fragmentparm	0		parm6, parm7, time * .06*parm6, time * .06*parm7	//scale x,y, scroll x,y of deform
		fragmentparm	1		.02		// deform amount
		fragmentparm	2		300		// exponent
	}
}

