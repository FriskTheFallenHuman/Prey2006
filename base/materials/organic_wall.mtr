textures/organic_wall/intestine
{
	qer_editorimage		textures/organic_wall/intestine
	matter_flesh
	{
	blend bumpmap
	map		textures/organic_wall/intestine_l
	}
	{
	blend diffusemap
	map		textures/organic_wall/intestine
	}
	{
	blend	specularmap
	map		textures/organic_wall/intestine_s
	specularexp 240, 30.2
	}
}

textures/organic_wall/groundmeat
{
	qer_editorimage		textures/organic_wall/groundmeat_d
	matter_flesh
	{
	blend bumpmap
	map			textures/organic_wall/groundmeat_local
	scroll time * 0, time / 30
	}
	{
	blend diffusemap
	map		textures/organic_wall/groundmeat_d
	scroll time * 0, time / 30
	}
	{
	blend	specularmap
	map		textures/organic_wall/groundmeat_s
	specularexp 340, 20.2
	scroll time * 0, time / 30
	}
	{
	blend gl_dst_color, gl_src_color
	map		textures/vpaint/feeding2_p
	scale 2,2
	rotate time / 500
	}
}

textures/organic_wall/puss
{
	qer_editorimage		textures/organic_wall/puss_proxy
	matter_flesh
	{
		blend bumpmap
		scroll time / -40, time * 0
		map		textures/organic_wall/puss_local
	}
	{
		blend diffusemap
		scroll time / -300, time * 0
		rotate time / -300
		rgb twostage [0.2 * time + 1 / 3]
		scale .5,.5
		map			textures/organic_wall/puss_d
	}
	{
		blend diffusemap
		scroll time / -100, time * 0
		rgb 0.7
		map			textures/organic_wall/puss_d
	}
	{
		blend specularmap
		map			textures/organic_wall/puss_s
		scroll time / -40, time * 0
	}
	{
		blend specularmap
		map		textures/organic_wall/puss_s
		rgb twostage [0.2 * time / 10]
		rotate time / -700
		scroll time / -210, time * 0
	}
}

