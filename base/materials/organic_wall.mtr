textures/organic_wall/bio_organic_sw1_dissolve
{
	qer_editorimage textures/organic_wall/bio_organic_sw2_proxy
	matter_metal
	noshadows

	bumpmap			textures/organic_wall/bio_organic_sw1_local
	diffusemap		textures/organic_wall/bio_organic_sw1_d
	specularmap		textures/organic_wall/bio_organic_sw1_s

	//global alphatest stages
	{
		if ( parm5 == 0 )
		blend none
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest 1 - ( time + parm4 ) * 0.5
	}
	{
		if ( parm5 > 0 ) //fade out
		blend none
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest ( time + parm4 ) * 0.5
	}

	//additive fx stage
	{
		if ( parm5 == 0 ) //fade out
		privatepolygonoffset	-1
		blend add
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest 1 - ( time + parm4 ) * 0.5 + 0.1
		rgb 2
		glowStage
	}
	{
		if ( parm5 > 0 ) //fade out
		privatepolygonoffset	-1
		blend add
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest ( time + parm4 ) * 0.5 - 0.1
		rgb 2
		glowStage
	}
}

