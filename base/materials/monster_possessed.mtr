models/monsters/girl_liquid/girl_liquid
{
	noShadows
//	noselfshadow
	unsmoothedtangents
	translucent
	matter_flesh
	collision

	renderbump -size 1024 1024 -trace 0.03 -colormap models/monsters/girl_liquid/girl_liquid_local models/monsters/girl_liquid/girl_liquidhi.lwo

/*	{ // Normal girl
		if ( parm7 == 0 ) && ( parm6 != 0 )
		blend diffusemap
		map models/monsters/girl_liquid/girl_liquid
		alphatest 0.5
	}*/

	{ // Possessed girl, about to burn out
//		if parm7 && ( parm6 == 0 )
		blend diffusemap
		map models/monsters/girl_liquid/girl_liquid
		rgb (ZeroToOneClamp [(parm4 + time) * 0.6])
	}
/*	{
		blend gl_dst_color, gl_src_color
		map models/monsters/girl_liquid/girl_liquid
	}
	{ // Possessed girl, not about to burn out
		if ( parm7 == 0 ) && ( parm6 == 0 )
		blend diffusemap
		map models/monsters/girl_liquid/girl_liquid
		alphatest 0.5
	}*/

	{ // Possessed girl vein additive
		if parm6 == 0
		blend add
		map models/monsters/girl_liquid/girl_liquid
		glowStage
		rgb 0.1
	}
	{ // Possessed girl vein additive
		if parm6 == 0
		blend add
		map models/monsters/girl_liquid/girl_veins
		glowStage
		rgb (ZeroToOneClamp [(parm4 + time) * 0.6])
	}
	{ // Possessed girl, about to burn out
		if parm7 && ( parm6 == 0 )
		blend add
		map _white
		red		0.058
		green	0.058
		blue	0.072
		glowStage
	}
	{ // Possessed girl eyes
		if parm6 == 0
		blend add
		map models/monsters/girl_liquid/girl_eyes
		privatePolygonOffset 1.5
		rgb 0.75 * crawlerBlink[ time * 0.2 ]
		glowStage
		ignoreAlphaTest
	}

	{
		blend	bumpmap
		map	addnormals(models/monsters/girl_liquid/girl_liquid_local, heightmap(models/monsters/girl_liquid/girl_liquid_h, 7 ) )
		highres
	}
	specularmap models/monsters/girl_liquid/girl_liquid_s

/*	{	// Burning flesh effect - burns away the diffuse map
		if	parm7			// only when dead

		// make a burned away alpha test for the normal skin
		blend	gl_zero, gl_one			// don't draw anything

		map models/monsters/girl_liquid/girl_liquid_dis	// replace this with a monster-specific texture
		alphaTest 0.05 + 1.4 * (time - parm7)
	}
	{	// burning corpse effect - blueish effect that trails the burnout
		if parm7			// only when dead

		// draw the fire burn at a negative polygonOffset, so it is behind the other stages
		privatePolygonOffset	-1		// stage-only polygon offset
        blend add
        blend gl_one, gl_zero

		map models/monsters/girl_liquid/girl_liquid_dis	// replace this with a monster-specific texture
		alphaTest 1.4 * (time - parm7)
		glowStage
	}
	{	// Remaining flesh, not alpha masked
		if parm7			// only when dead

		// draw the fire burn at a negative polygonOffset, so it is behind the other stages
		privatePolygonOffset	-1.3		// stage-only polygon offset - ensure that this is lower than the blue burn
        blend gl_zero, gl_zero
        blend gl_one, gl_zero

		map models/monsters/girl_liquid/girl_liquid_dis_muscle	// replace this with a monster-specific texture
//		alphaTest 1.0 * (time - parm7)
//		alphaTest 0 // Necessary, otherwise the other stages will mask out this stage
	}
	{	// burning corpse effect - blueish effect that trails the burnout
		if parm7			// only when dead

		// draw the fire burn at a negative polygonOffset, so it is behind the other stages
		privatePolygonOffset	-1.6		// stage-only polygon offset
//        blend add
        blend gl_one, gl_zero

		map models/monsters/girl_liquid/girl_liquid_dis_muscle	// replace this with a monster-specific texture
		alphaTest 1.5 * (time - parm7 + 0.2)
//		glowStage
	}*/


	{ // Scope view texture
		blend add
		map textures/sfx/monsterSniperView
		texgen screen
		scroll 0.5, 0.5
		scale 0.5, 0.5
		clamp
		glowStage
		scopeView	// Only visible in the rifle scope view
	}
}

