// girl_liquid
//
//////////////////////////////////////////////////////////////////////////////

models/monsters/girl_liquid/girl_liquid_normal
{
	noShadows
//	noselfshadow
	unsmoothedtangents
	matter_flesh

	renderbump -size 1024 1024 -trace 0.03 -colormap models/monsters/girl_liquid/girl_liquid_local models/monsters/girl_liquid/girl_liquidhi.lwo

	{ // Normal girl
		blend diffusemap
		map models/monsters/girl_liquid/girl_liquid
		alphatest 0.5
	}
	{
		blend	bumpmap
		map	addnormals(models/monsters/girl_liquid/girl_liquid_local, heightmap(models/monsters/girl_liquid/girl_liquid_h, 7 ) )
		highres
	}
	specularmap models/monsters/girl_liquid/girl_liquid_s

	{ // Scope view texture
		blend add
		map textures/sfx/monsterSniperView
		texgen screen
		scroll 0.5, 0.5
		scale 0.5, 0.5
		clamp
		glowStage
		scopeView	// Only visible in the rifle scope view
	}
}

