textures/particles/smokey
{
	qer_editorimage textures/particles/smokey
	noshadows
	{
		if fragmentprograms == 0
		blend	gl_src_alpha, gl_one_minus_src_alpha
		map textures/particles/smokey
		vertexcolor
		colored
	}
	{
		if fragmentprograms == 1
		maskDepth
		blend	gl_src_alpha, gl_one_minus_src_alpha
		program	depthsprite.vfp

		fragmentmap		0		textures/particles/smokey
		fragmentmap		2		textures/particles/smokey_depth
	}
}

