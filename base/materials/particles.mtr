textures/particles/lightning
{
	qer_editorimage textures/particles/lightning
	translucent
	noshadows
	{
		maskcolor
		map	makealpha(textures/particles/lightning_mask)
		glowstage
	}
	{
		maskalpha
		blend	gl_dst_alpha, gl_one
		map textures/particles/lightning
		scroll (time / 0), (time * 50)
		vertexcolor
		colored
		glowstage
	}
}

textures/particles/lightning_reverse
{
	qer_editorimage textures/particles/lightning
	translucent
	noshadows
	{
		maskcolor
		map	makealpha(textures/particles/lightning_mask)
		glowstage
	}
	{
		maskalpha
		blend	gl_dst_alpha, gl_one
		map textures/particles/lightning
		scroll (time / 0), (time * -50)
		vertexcolor
		centerscale 1, 2
		colored
		glowstage
	}
}

textures/particles/lightning_flicker
{
	qer_editorimage textures/particles/lightning
	translucent
	noshadows
	{
		maskcolor
		map	makealpha(textures/particles/lightning_mask)
		glowstage
	}
	{
		maskalpha
		blend	gl_dst_alpha, gl_one
		map textures/particles/lightning
		scroll (time / 0), (time * 50)
		vertexcolor
		color
			parm0 * ( jenflash2 [time * 5]),
			parm1 * ( jenflash2 [time * 5]),
			parm2 * ( jenflash2 [time * 5]), 0
		glowstage
	}
}

