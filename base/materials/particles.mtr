textures/particles/dryice_fade_opt {
	qer_editorimage textures/particles/smokepuff11
	translucent
	noshadows
	twosided
	sort		10

	{if ( distance < ( 400 + parm5 ) )
		blend gl_one_minus_src_alpha, gl_one_minus_src_color //graying blend (white smoke)
		map textures/particles/dry_ice
		vertexcolor
		rgb 0
//		glowStage
	}
	{if ( distance <= ( 800 + parm6 ) && distance >= ( 400 + parm5 ) )
		blend gl_one_minus_src_alpha, gl_one_minus_src_color //graying blend (white smoke)
		map textures/particles/dry_ice
		vertexcolor
		rgb 0 + ( ( distance - (400 + parm5) ) / ( (800 + parm6) - (400 + parm5) ) )//dynamic interaction explanation[ (current distance - base) / (max distance - base) = a number between 0 'close' and 1 'far'. 1 - this number = rgb
//		glowStage
	}
	{if ( distance > 800 + parm6 )
		blend gl_one_minus_src_alpha, gl_one_minus_src_color //graying blend (white smoke)
		map textures/particles/dry_ice
		vertexcolor
		rgb 1
//		glowStage
	}
}

