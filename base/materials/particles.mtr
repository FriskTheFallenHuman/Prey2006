textures/particles/vortex4_spirit
{
	qer_editorimage textures/particles/vortex4
	translucent
	noshadows
	twosided


	{	if ( distance < ( 200 ) )

		blend gl_one_minus_dst_color, gl_one
		map textures/particles/vortex4
		glowStage
		rgb 1
		spiritWalk
		vertexcolor

	}
	{if ( distance <= ( 400 ) && distance >= ( 200 ) )

		blend gl_one_minus_dst_color, gl_one
		map textures/particles/vortex4
		glowStage
		rgb  1 - ( ( distance - (200 ) ) / ( (400 ) - (200 ) ) )
		spiritWalk
		vertexcolor


	}
	{if ( distance > 400 )
		blend gl_one_minus_dst_color, gl_one
		map textures/particles/vortex4
		glowStage
		rgb 0
		spiritWalk
		vertexcolor
	}
}

