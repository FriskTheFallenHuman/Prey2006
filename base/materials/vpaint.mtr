		VertexColor
	}
	{
		blend		specularmap
		scale 1.,1.
		map		textures/vpaint/feeding4_sx
		VertexColor
	}
	{
		blend		add
		scale 1.,1.
		map		textures/vpaint/feeding2_sphere_add
		VertexColor
		glowStage
		rgb alpha[(time * 0.17) + .2]
	}
	// Black verts
	{
	//	shaderFallback2
		blend	bumpmap
		scale .75,.75
		map		textures/organic_floor/sphere_husk_local
	}
	{
	//	shaderFallback2
		blend	diffusemap
		scale .75,.75
		inversevertexcolor
		map		textures/organic_floor/sphere_husk_d
	}
	{
	//	shaderFallback2
		blend	specularmap
		scale .75,.75
		inversevertexcolor
		map		textures/organic_floor/sphere_husk_s
	}

	{
		if parm7
		blend gl_zero, gl_one
		map textures/sfx/genericdissolve
		alphaTest 0.05 + 0.35 * (time - parm4)
		centerscale 0.5, 0.5
	}
	{
		if parm7
		privatePolygonOffset -1
        blend add
		map textures/sfx/genericdissolve
		alphaTest 0.35 * (time - parm4)
		centerscale 0.5, 0.5
		glowStage
	}
	{
		if parm7
		privatePolygonOffset -1.3
		blend add
		map _white
		alphaTest 0
		glowStage
	}

