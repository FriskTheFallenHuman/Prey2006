//This is the material file for Vertex painted surfaces that reference
//two shaders.

//-----------------------------------------------------------------------
// Feeding tower blend materials
//-----------------------------------------------------------------------
models/mapobjects/fmisc/fuv_blend
{
	matter_flesh
	qer_editorimage textures/editor/checker512
	noselfshadow
	// White verts
	{
		blend	bumpmap
		scale parm4,parm5
		map heightmap(textures/feedingtower/bone3_h, 1)
	}
	{
		blend diffusemap
		scale parm4,parm5
		vertexcolor
		map		textures/feedingtower/bone3_d
	}
	{
		blend specularmap
		scale parm4,parm5
		vertexcolor
		map		textures/feedingtower/bone3_s
	}
	// Black verts
	{
		blend	bumpmap
		scale parm6,parm7
		map
		addnormals( textures/feedingtower/ftworg1_local,
		heightmap( textures/feedingtower/ftworg1_b, 2.0 ))
	}
	{
		blend	diffusemap
		scale parm6,parm7
		inversevertexcolor
		map	textures/feedingtower/ftworg1
	}
	{
		blend	specularmap
		scale parm6,parm7
		inversevertexcolor
		map	textures/feedingtower/ftworg1_spec
	}
}


