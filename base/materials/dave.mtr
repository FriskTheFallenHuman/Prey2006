textures/dch/ltcone_rayhive
{
	qer_editorimage textures/sfx/lightcone3
	translucent
	noshadows
	nonsolid
	noimpact
	twoSided
	{
		blend	add
		map		textures/sfx/lightcone3
		scroll	time * 0.030, 0.0
		color	0.35, 0.28, 0.24, 0.0
		linear
	}
	{
		blend	add
		map		textures/sfx/lightcone3
		scroll	time * -0.035, 0.0
		color	0.35, 0.28, 0.24, 0.0
		linear
	}
}


