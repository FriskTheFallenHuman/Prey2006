lights/dch/defProjLt_pulse1
{
	lightFalloffImage	makeintensity( lights/squarelight1a )
	{
		map	lights/defaultpointlight
		zeroClamp
		color		parm0 * (.25 * sintable [0 + time * 2 ] + 2),
				parm1 * (.25 * sintable [0 + time * 2 ] + 2),
				parm2 * (.25 * sintable [0 + time * 2 ] + 2),
				1.0
	}
}

lights/dch/defProjLt_pulse1ALT
{
	lightFalloffImage	makeintensity( lights/squarelight1a )
