textures/vpaint/feeding2_3_a

{
	matter_flesh
	qer_editorimage textures/vpaint/feeding2_3_proxy
	noselfshadow
	// Black verts
	{
		shaderFallback2
		blend	bumpmap
		scale parm6,parm7
		map	textures/vpaint/feeding3_local
	}
	{
		shaderFallback2
		blend	diffusemap
		scale parm6,parm7
		inversevertexcolor
		map	add( textures/vpaint/feeding3_d, textures/vpaint/feeding3_ss)
	}
	{
		shaderFallback2
		blend	specularmap
		scale parm6,parm7
		inversevertexcolor
		map	textures/vpaint/feeding3_s
	}
	{
		shaderLevel2
		inversevertexcolor

		blend shader
		vertexprogram	vertexscale.vp
		fragmentprogram parallaxExp.vfp

		vertexparm		0		parm6,parm7,0,0

		fragmentmap		1		textures/vpaint/feeding3_local
		fragmentmap		4		add( textures/vpaint/feeding3_d, textures/vpaint/feeding3_ss)
		fragmentmap		5		textures/vpaint/feeding2_s2
		fragmentmap		7		textures/vpaint/feeding3_p

		fragmentparm	0		.16, -.08
		fragmentparm	1		300		// exponent
	}
	// White verts
	{
		shaderFallback2
		blend	bumpmap
		scale parm4,parm5
		map		textures/vpaint/feeding2_local
	}
	{
		shaderFallback2
		blend diffusemap
		scale parm4,parm5
		vertexcolor
		map		add( textures/vpaint/feeding2_d, textures/vpaint/feeding2_ss)
	}
	{
		shaderFallback2
		blend specularmap
		scale parm4,parm5
		vertexcolor
		map		textures/vpaint/feeding2_s
	}
	{
		shaderLevel2
		vertexcolor

		blend shader
		vertexprogram	vertexscale.vp
		fragmentprogram parallaxExp.vfp

		vertexparm		0		parm4,parm5,0,0

		fragmentmap		1		textures/vpaint/feeding2_local
		fragmentmap		4		add( textures/vpaint/feeding2_d, textures/vpaint/feeding2_ss)
		fragmentmap		5		textures/vpaint/feeding2_s2
		fragmentmap		7		textures/vpaint/feeding2_p

		fragmentparm	0		.12, -.06
		fragmentparm	1		300		// exponent
	}
}


