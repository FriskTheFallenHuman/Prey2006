textures/feedingtower/oozetube2
{
	matter_flesh
	detail
		qer_editorimage textures/feedingtower/oozetube2_local
		bumpmap 		addnormals (textures/feedingtower/oozetube2_local, heightmap (textures/feedingtower/oozetube2_h, 6))
		diffusemap	 	textures/feedingtower/oozetube2_d
		specularmap		textures/feedingtower/oozetube2_s
	{
		blend add
			map textures/feedingtower/oozetube2_add1
			scroll (0.1 * sintable [time * 0.1]), time * 0.3
			//		rgb (jitter2 [time * 4])
	}
	{
		blend add
			map textures/feedingtower/oozetube2_add2
			scroll 0, cbloodflow[time * 0.25] - time * 0.3
			//		rgb (jitter2 [time * 6])
	}
	{
		blend add
			map textures/feedingtower/oozetube2_add3
			scroll (0.2 * sintable[time * 0.2]), 2 * cbloodflow[time * 0.25] - time * 0.3
			rgb (jitter2 [time * 1])
	}
}

