textures/feedingtower/orgfloor1scroll
{
	matter_flesh
	qer_editorimage 	textures/feedingtower/orgfloor1_d

	{
		blend bumpmap
		map addnormals (textures/feedingtower/orgfloor1_local,  heightmap (textures/feedingtower/orgfloor1_h, 4))
		scroll time * 0.1, (0.1 * sintable [time* 0.1])
		scale (jitter3 [time * .85]), (jitter3 [time * .85])
	}
	{
		blend diffusemap
		map textures/feedingtower/orgfloor1_d
		scroll time * 0.1, (0.1 * sintable [time* 0.1])
		scale (jitter3 [time * .85]), (jitter3 [time * .85])
	}
	{
		blend specularmap
		map textures/feedingtower/orgfloor1_s
		scroll time * 0.1, (0.1 * sintable [time* 0.1])
		scale (jitter3 [time * .85]), (jitter3 [time * .85])
	}
}

