textures/feedingtower/orgwall2_pulse
{
	matter_flesh
	qer_editorimage 	textures/feedingtower/orgwall2_d

	{
		blend bumpmap
		map addnormals (textures/feedingtower/orgwall2_local,  heightmap (textures/feedingtower/orgwall2_h, 4))
		scale (jitter3 [time * .15]), (jitter3 [time * .15])
	}
	{
		blend diffusemap
		map textures/feedingtower/orgwall2_d
		scale (jitter3 [time * .15]), (jitter3 [time * .15])
	}
	{
		blend specularmap
		map textures/feedingtower/orgwall2_s
		scale (jitter3 [time * .15]), (jitter3 [time * .15])
	}
}


