textures/organic_light/biopulse1_add1
{
	qer_editorimage 	textures/organic_light/orglightpatch1_d
	decal_alphatest_macro

	bumpmap 		addnormals (textures/organic_light/orglightpatch1_local,
				heightmap (textures/organic_light/orglightpatch1_h, 6))
	specularmap		textures/organic_light/orglightpatch1_s
	{
		blend diffusemap
		map textures/organic_light/orglightpatch1_d
		alphatest	0.5
	}
	{
		blend		add
		map		textures/organic_light/orglightpatch1_add
		color		( parm0 * parm5 ) + (( 1 - parm5 ) * parm0  * ((( biopulse1 [ parm3 + ( time * parm6 ) ] +1) * 0.5 ))),
				( parm1 * parm5 ) + (( 1 - parm5 ) * parm1  * ((( biopulse1 [ parm3 + ( time * parm6 ) ] +1) * 0.5 ))),
				( parm2 * parm5 ) + (( 1 - parm5 ) * parm2  * ((( biopulse1 [ parm3 + ( time * parm6 ) ] +1) * 0.5 ))),
				1.0
	}

}

