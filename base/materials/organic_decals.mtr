textures/organic_trim/circuitwall1_a_mask
{


	qer_editorimage 	textures/organic_wall/circuitwall1_d
	decal_alphatest_macro

	bumpmap 		addnormals (textures/organic_wall/circuitwall1_local,
				heightmap (textures/organic_wall/circuitwall1_h, 4))

	specularmap		textures/organic_wall/circuitwall1_s
	{
		blend diffusemap
		map textures/organic_wall/circuitwall1_d
		alphatest 0.2
	}
}

textures/organic_trim/circuitwall1_a_mask_add
{


	qer_editorimage 	textures/organic_wall/circuitwall1_d
	decal_alphatest_macro

	bumpmap 		addnormals (textures/organic_wall/circuitwall1_local,
				heightmap (textures/organic_wall/circuitwall1_h, 4))

	specularmap		textures/organic_wall/circuitwall1_s
	{
		blend diffusemap
		map textures/organic_wall/circuitwall1_d
		alphatest 0.2
	}
	{
		blend add
		map textures/organic_wall/circuitwall1_add_white
		color	parm0 * ((jitter1 [time * 2]) - 0.1),
			parm1 * ((jitter1 [time * 2]) - 0.1),
			parm2 * ((jitter1 [time * 2]) - 0.1),
			1.0
	}
}

