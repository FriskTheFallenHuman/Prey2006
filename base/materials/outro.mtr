textures/outro/earth_base
{
	noshadows
	{
		// diffuse
		blend	diffusemap
		map		textures/outro/earthcomp4096
		highres
	}
	{
		// spec
		blend specularmap
		map		textures/outro/earth2048s
		specularexp 10, 1
		color	0.17, 0.19, 0.2, 0
		highres
	}
	{
		// night city lights
		maskcolor
		map		textures/outro/earthcomp4096
		highres
	}
	{
		blend	gl_dst_alpha, gl_one
		map		_white
		colored
		highres
	}
}

