textures/outro/sun
{
	qer_editorimage textures/outro/sun
	deform sprite
	{
		blend		add
		//blend	gl_src_alpha, gl_one_minus_src_alpha
		map textures/outro/sun
		clamp
		glowstage
		rgb 0.8 * (pulsebeat [time * 0.1]) * parm4
	}
	{
		blend		add
		map textures/outro/sun
		rotate time * -0.001
		rotate 1.3
		clamp
		glowstage
		rgb 0.1 * (pulsebeat [time * -0.3]) * parm4
	}
	{
		maskcolor
		map	makealpha(textures/outro/sun_mask)
		rotate time * -0.001
		centerscale 0.98 * (pulsebeat2 [time * 0.05]), 0.98 * (pulsebeat2 [time * 0.05])
		clamp
		glowstage
	}
	{
		blend	gl_dst_alpha, gl_one
		map textures/outro/sun_surface
		scroll time * -0.005, time * -0.005
		rotate time * 0.004
		centerscale 1.1, 1.1
		rgb 0.4 * (pulsebeat [time * 0.3]) * parm4
	}
	{
		blend	gl_dst_alpha, gl_one
		map textures/outro/sun_surface
		scroll time * 0.005, time * 0.005
		rotate time * -0.004
		centerscale 0.8, 0.8
		rgb 0.35 * (pulsebeat [time * -0.1]) * parm4
	}
}

