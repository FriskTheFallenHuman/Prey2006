//---------------------
//	multiplayer yuppies
//---------------------

models/characters/yuppiebody/body_mp
{
//	noselfshadow
	unsmoothedtangents
	matter_flesh

	renderbump  -size 1024 1024 -aa 1  -colormap models/characters/yuppiebody/body_local models/characters/yuppiebody/body/davidhi.lwo
	diffusemap models/characters/yuppiebody/body
	bumpmap	addnormals(models/characters/yuppiebody/body_local, heightmap(models/characters/yuppiebody/body_h, 6 ) )
	specularmap models/characters/yuppiebody/body_s
	{ // Scope view texture
		blend add
		map textures/sfx/monsterSniperView
		texgen screen
		scroll 0.5, 0.5
		scale 0.5, 0.5
		clamp
		glowStage
		scopeView	// Only visible in the rifle scope view
	}
	//HUMANHEAD rww - MP STAGES BEGIN
	{
		blend add
		map textures/sfx/mp_damageoverlay
		texgen screen
		scroll sintable[time], costable[time]
		scale 2.5, 2.5

		red		mpdamage_r[parm3]*mpdamage_f[(time*2)+parm5]
		blue	mpdamage_b[parm3]*mpdamage_f[(time*2)+parm5]
		green	mpdamage_g[parm3]*mpdamage_f[(time*2)+parm5]

		glowStage
	}
	//HUMANHEAD END
}

