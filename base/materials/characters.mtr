////////////////////////////////////////////////////////////////////////////////////////////////////
//
// girlfriend_liquid or Jenny
//
////////////////////////////////////////////////////////////////////////////////////////////////////

models/characters/girlfriend/girlfriend_liquid
{
	noShadows
//	noselfshadow
	unsmoothedtangents
	matter_flesh
	noimpact

	renderbump -size 1024 1024 -trace 0.03 -colormap models/characters/girlfriend/girlfriend_liquid_body_local models/characters/girlfriend/girlfriend_liquidhi_body.lwo

	// Non-fragment program stages
	{
		shaderFallback3
		blend diffusemap
		map models/characters/girlfriend/girlfriend_liquid
	}
	{
		shaderFallback3
		blend bumpmap
		map addnormals(models/characters/girlfriend/girlfriend_liquid_local, heightmap(models/characters/girlfriend/girlfriend_liquid_h, 7 ) )
		highres
	}
	{
		shaderFallback3
		blend specularmap
		map models/characters/girlfriend/girlfriend_liquid_s
	}

	// Fragment program stages
	{
		shaderLevel3
		blend shader
		program skin.vfp

		fragmentparm	0	1, 0, 0, 0		// mask channel
		// fragmentparm	1	0.4, 0.0, .0		// sss color
		fragmentparm	1	0.75, 0.3, 0.3		// sss color
		fragmentparm	2	0.5, 0.5		// wrap around
		fragmentparm	3	1.5				// rim
		fragmentparm	4	0.18, 0.18, 0.22, 12		// specular color, power
		fragmentparm	5	0.5, 0.5, 0.55, 8		// glancing specular color, power
		fragmentparm	6	0.15

		//fragmentmap 0 uncompressed clamp textures/common/orenmap
		fragmentmap 1 highres addnormals(models/characters/girlfriend/girlfriend_liquid_local, heightmap(models/characters/girlfriend/girlfriend_liquid_h, 7 ) )
		fragmentmap 4 models/characters/girlfriend/girlfriend_liquid
		fragmentmap 5 models/characters/girlfriend/girlfriend_liquid_s_skin
		fragmentmap 6 models/characters/girlfriend/girlfriend_liquid_sg_skin
		fragmentmap 7 models/characters/girlfriend/girlfriend_liquid_mask
	}
	{
		shaderLevel3
		blend shader
		program cloth.vfp

		fragmentparm	0	0, 0, 1, 0		// mask channel

		// I recommend leaving these settings
		fragmentparm	1	0.3, 0.6		// scatter, diffuse
		fragmentparm	2	1.5, 1.5, 0.5, 1.2		// side light fuzz, fuzz sharpness, view fuzz, fuzz power
		fragmentparm	3	0.5					// fuzz amount

		fragmentmap 1 highres addnormals(models/characters/girlfriend/girlfriend_liquid_local, heightmap(models/characters/girlfriend/girlfriend_liquid_h, 7 ) )
		fragmentmap 4 models/characters/girlfriend/girlfriend_liquid
