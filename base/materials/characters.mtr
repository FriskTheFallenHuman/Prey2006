models/characters/human3/human3_body
{
	noShadows
//	noselfshadow
	unsmoothedtangents
	matter_flesh
	forceoverlays

	renderbump  -size 1024 1024 -aa 1  -colormap models/characters/human3/human3_body_local models/characters/human2/human2_hi.lwo
	//diffusemap models/characters/human3/human3_body
	//bumpmap	addnormals(models/characters/human3/human3_body_local, heightmap(models/characters/human3/human3_body_h, 3 ) )
	//specularmap models/characters/human3/human3_body_s
	{
		shaderfallback2
		blend specularmap
		map models/characters/human3/human3_body_s
		highres
	}

	{
		shaderlevel2
		blend shader
		program interactionexp.vfp

		fragmentparm 0	 350, 22

		fragmentmap 1			highres addnormals(models/characters/human3/human3_body_local, heightmap(models/characters/human3/human3_body_h, 3 ) )
		fragmentmap 4			models/characters/human3/human3_body
		fragmentmap 5			models/characters/human3/human3_body_s
	}
	{
		shaderfallback2
		blend diffusemap
		map models/characters/human3/human3_body
		highres
	}
	{
		shaderfallback2
		blend bumpmap
		map addnormals(models/characters/human3/human3_body_local,
				heightmap(models/characters/human3/human3_body_h, 3 ) )
		highres
	}

	{	// Burning flesh effect - burns away the diffuse map
		if	parm7			// only when dead
		// make a burned away alpha test for the normal skin
		blend	gl_zero, gl_one			// don't draw anything
		map textures/sfx/genericdissolve	// replace this with a monster-specific texture
		alphaTest 0.1 + 0.9 * (time - parm7)
		centerscale 0.30, 0.30
	}
	{	// burning corpse effect - blueish effect that trails the burnout
		if parm7			// only when dead
		// draw the fire burn at a negative polygonOffset, so it is behind the other stages
		privatePolygonOffset	-1		// stage-only polygon offset
        blend gl_one, gl_zero
		map textures/sfx/genericdissolve	// replace this with a monster-specific texture
		alphaTest 0.9 * (time - parm7)
		red		0.6
		green	0.6
		blue	1.0
		glowStage
		centerscale 0.30, 0.30
	}

	{ // Scope view texture
		blend add
		map textures/sfx/monsterSniperView
		texgen screen
		scroll 0.5, 0.5
		scale 0.5, 0.5
		clamp
		glowStage
		scopeView	// Only visible in the rifle scope view
	}
}

