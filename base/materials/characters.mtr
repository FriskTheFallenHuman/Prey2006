models/monsters/grandfather_liquid/grandfather_liquid
{
	noShadows
//	noselfshadow
	unsmoothedtangents
	matter_flesh
	noimpact
	forceoverlays

	renderbump -size 1024 1024 -trace 0.03 -colormap models/monsters/grandfather_liquid/grandfather_liquid_local2 models/monsters/grandfather_liquid/grandfather_liquidhi_body.lwo

	// Non-fragment program stages
	{
		shaderfallback3
		blend diffusemap
		map models/monsters/grandfather_liquid/grandfather_liquid
	}
	{
		shaderfallback3
		blend bumpmap
		map addnormals(models/monsters/grandfather_liquid/grandfather_liquid_local, heightmap(models/monsters/grandfather_liquid/grandfather_liquid_h, 7 ) )
		highres
	}
	{
		shaderfallback3
		blend specularmap
		map models/monsters/grandfather_liquid/grandfather_liquid_s
	}

	// Fragment program stages
	{
		shaderLevel3
		blend shader
		program skin.vfp

		fragmentparm	0	1, 0, 0, 0		// mask channel
		// fragmentparm	1	0.2, 0.0, .0		// sss color
		fragmentparm	1	0.6, 0.2, 0.2		// sss color
		fragmentparm	2	0.5, 0.5		// wrap around
		fragmentparm	3	1.5				// rim
		fragmentparm	4	0.2, 0.2, 0.25, 12		// specular color, power
		fragmentparm	5	0.39, 0.39, 0.42, 10		// glancing specular color, power
		fragmentparm	6	0.15

		fragmentmap 1 highres addnormals(models/monsters/grandfather_liquid/grandfather_liquid_local, heightmap(models/monsters/grandfather_liquid/grandfather_liquid_h, 7 ) )
		fragmentmap 4 models/monsters/grandfather_liquid/grandfather_liquid
		fragmentmap 5 models/monsters/grandfather_liquid/grandfather_liquid_s_skin
		fragmentmap 6 models/monsters/grandfather_liquid/grandfather_liquid_s_skin
		fragmentmap 7 models/monsters/grandfather_liquid/grandfather_liquid_mask
	}
	{
		shaderLevel3
		blend shader
		program cloth.vfp

		fragmentparm	0	0, 0, 1, 0		// mask channel

		// I recommend leaving these settings
		//fragmentparm	1	0, 0.99		// scatter, diffuse
		//fragmentparm	2	1.2, 1.5, 0.5, 1.8		// side light fuzz, fuzz sharpness, view fuzz, fuzz power
		//fragmentparm	3	0.1					// fuzz amount

		fragmentparm	1	0.1, 0.8		// scatter, diffuse
		fragmentparm	2	1.3, 1.5, 0.5, 1.2		// side light fuzz, fuzz sharpness, view fuzz, fuzz power
		fragmentparm	3	0.3					// fuzz amount

		fragmentmap 1 highres addnormals(models/monsters/grandfather_liquid/grandfather_liquid_local, heightmap(models/monsters/grandfather_liquid/grandfather_liquid_h, 7 ) )
		fragmentmap 4 models/monsters/grandfather_liquid/grandfather_liquid
		//fragmentmap 6 models/characters/grandfather/grandfather_liquid_mask
		fragmentmap 6 models/monsters/grandfather_liquid/grandfather_liquid_mask
	}
	{
		shaderLevel3
		blend shader
		program interactionMasked.vfp

		fragmentparm	0	0, 1, 0, 0		// mask channel

		fragmentmap 1 highres addnormals(models/monsters/grandfather_liquid/grandfather_liquid_local, heightmap(models/monsters/grandfather_liquid/grandfather_liquid_h, 7 ) )
		fragmentmap 4 models/monsters/grandfather_liquid/grandfather_liquid
		fragmentmap 5 models/monsters/grandfather_liquid/grandfather_liquid_s
		fragmentmap 7 models/monsters/grandfather_liquid/grandfather_liquid_mask
	}

	//global alphatest stage
	{
		if ( parm5 > 0 ) //fade out
		blend none
		map textures/sfx/roadhouse_ramp_fx_256
		alphaTest ( time + parm4 ) * global6
	}

	//additive fx stage
	{
		if ( parm5 > 0 ) //fade out
		privatepolygonoffset	-1
		blend add
		map textures/sfx/roadhouse_ramp_fx_256
		alphaTest ( time + parm4 ) * global6 - global7
		rgb 2
		glowStage
	}
	{ // Scope view texture
		blend add
		map textures/sfx/monsterSniperView
		texgen screen
		scroll 0.5, 0.5
		scale 0.5, 0.5
		clamp
		glowStage
		scopeView	// Only visible in the rifle scope view
	}
}

