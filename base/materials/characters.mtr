models/characters/girlfriend/blonde
{
	noShadows
//	noselfshadow
	unsmoothedtangents
	matter_flesh

	renderbump -size 1024 1024 -trace 0.03 -colormap models/characters/girlfriend/girlfriend_liquid_body_local models/characters/girlfriend/girlfriend_liquidhi_body.lwo

	// Non-fragment program stages
	{
		shaderFallback3
		blend diffusemap
		map models/characters/girlfriend/girlfriend_liquid_blonde
	}
	{
		shaderFallback3
		blend bumpmap
		map addnormals(models/characters/girlfriend/girlfriend_liquid_local_blonde, heightmap(models/characters/girlfriend/girlfriend_liquid_h, 7 ) )
		highres
	}
	{
		shaderFallback3
		blend specularmap
		map models/characters/girlfriend/girlfriend_liquid_s_blonde
	}

	// Fragment program stages
	{
		shaderLevel3
		blend shader
		program skin.vfp

		fragmentparm	0	1, 0, 0, 0		// mask channel
		// fragmentparm	1	0.4, 0.0, .0		// sss color
		fragmentparm	1	0.75, 0.3, 0.3		// sss color
		fragmentparm	2	0.5, 0.5		// wrap around
		fragmentparm	3	1.5				// rim
		fragmentparm	4	0.18, 0.18, 0.22, 12		// specular color, power
		fragmentparm	5	0.5, 0.5, 0.55, 8		// glancing specular color, power
		fragmentparm	6	0.15

		//fragmentmap 0 uncompressed clamp textures/common/orenmap
		fragmentmap 1 highres addnormals(models/characters/girlfriend/girlfriend_liquid_local_blonde, heightmap(models/characters/girlfriend/girlfriend_liquid_h, 7 ) )
		fragmentmap 4 models/characters/girlfriend/girlfriend_liquid_blonde
		fragmentmap 5 models/characters/girlfriend/girlfriend_liquid_s_skin_blonde
		fragmentmap 6 models/characters/girlfriend/girlfriend_liquid_sg_skin_blonde
		fragmentmap 7 models/characters/girlfriend/girlfriend_liquid_mask_blonde
	}
	{
		shaderLevel3
		blend shader
		program cloth.vfp

		fragmentparm	0	0, 0, 1, 0		// mask channel

		// I recommend leaving these settings
		fragmentparm	1	0.3, 0.6		// scatter, diffuse
		fragmentparm	2	1.5, 1.5, 0.5, 1.2		// side light fuzz, fuzz sharpness, view fuzz, fuzz power
		fragmentparm	3	0.5					// fuzz amount

		fragmentmap 1 highres addnormals(models/characters/girlfriend/girlfriend_liquid_local_blonde, heightmap(models/characters/girlfriend/girlfriend_liquid_h, 7 ) )
		fragmentmap 4 models/characters/girlfriend/girlfriend_liquid_blonde
		//fragmentmap 5 models/characters/girlfriend/girlfriend_liquid_s_skin
		fragmentmap 6 models/characters/girlfriend/girlfriend_liquid_mask_blonde
		//fragmentmap 7 models/characters/girlfriend/girlfriend_liquid_cloth
	}
	{
		shaderLevel3
		blend shader
		program interactionMasked.vfp

		fragmentparm	0	0, 1, 0, 0		// mask channel

		fragmentmap 1 highres addnormals(models/characters/girlfriend/girlfriend_liquid_local_blonde, heightmap(models/characters/girlfriend/girlfriend_liquid_h, 7 ) )
		fragmentmap 4 models/characters/girlfriend/girlfriend_liquid_blonde
		fragmentmap 5 models/characters/girlfriend/girlfriend_liquid_s_blonde
		fragmentmap 7 models/characters/girlfriend/girlfriend_liquid_mask_blonde
	}
	{ // Scope view texture
		blend add
		map textures/sfx/monsterSniperView
		texgen screen
		scroll 0.5, 0.5
		scale 0.5, 0.5
		clamp
		glowStage
		scopeView	// Only visible in the rifle scope view
	}
}

