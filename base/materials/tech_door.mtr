textures/tech_door/biotechdoor3_add
{
	qer_editorimage 	textures/tech_door/biotechdoor3_d
	matter_altmetal

	bumpmap 		addnormals (textures/tech_door/biotechdoor3_local,
				heightmap (textures/tech_door/biotechdoor3_h, 6))

	diffusemap	 	textures/tech_door/biotechdoor3_d
	specularmap		textures/tech_door/biotechdoor3_s

//	{ if ( parm7 == 0 && parm6 == 0 )
//		blend add
//		map textures/tech_door/biotechdoor3_add
//		color
//		parm0 * ((jitter1 [time * 2])),
//		parm1 * ((jitter1 [time * 2])),
//		parm2 * ((jitter1 [time * 2])),
//		1.0
//	}

	{ if ( parm7 < 2 && parm6 == 0)
		blend add
		map textures/tech_door/biotechdoor3_add
		red 	0.35 * ((jitter1 [time * 2]))
		green	0.83 * ((jitter1 [time * 2]))
		blue 	1	 * ((jitter1 [time * 2]))	// cdoor_add_open
	}

	{ if ( parm7 < 2 && parm6 == 1 )
		blend add
		map textures/tech_door/biotechdoor3_add
		red 	0.35	* ( airlockwaittable [ time * 0.8 ] )		// cdoor_add_open_waiting
		green	0.83 	* ( airlockwaittable [ time * 0.8 ] )	// cdoor_add_open_waiting
		blue 	1		* ( airlockwaittable [ time * 0.8 ] )		// cdoor_add_open_waiting
	}

	{ if ( parm7 == 2 && parm6 == 0 )
		blend add
		map textures/tech_door/biotechdoor3_add
		red 0.884 	* ((jitter1 [time * 2]))
		green  0.0 	* ((jitter1 [time * 2]))
		blue 0.0	* ((jitter1 [time * 2]))	// locked
	}

	{ if ( parm7 == 2 && parm6 == 1 )
		blend add
		map textures/tech_door/biotechdoor3_add
		red 0.884 * ( airlockwaittable [ time * 0.8 ] )	//locked
		green  0.0
		blue 0.0		// cdoor_add_neveropen
	}

	{ if ( parm7 == 3 )
		blend add
		map textures/tech_door/biotechdoor3_add
		red 	0
		green	0
		blue	0
	}
}

