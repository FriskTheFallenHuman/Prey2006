models/monsters/mutate/fluidtube
{
	noShadows
//	noselfshadow
	unsmoothedtangents
	matter_glass
	translucent
	twosided
	forceOverlays

	renderbump -size 16 64 -aa 0 -colormap models/monsters/mutate/fluidtube_local models/monsters/mutate/mutatehi.lwo
//	diffusemap	models/monsters/mutate/fluidtube
	{
		if ( parm7 == 0 )
		blend diffusemap
		map	models/monsters/mutate/fluidtube
	}
	{
		if parm7
		blend diffusemap
		map	_black
	}
//	bumpmap	models/monsters/mutate/fluidtube_local
//	specularmap	models/monsters/mutate/fluidtube_s
	{
		if ( parm7 == 0 )
		blend specularmap
		map	models/monsters/mutate/fluidtube_s
	}
	{
		if parm7
		blend specularmap
		map	_black
	}
	{
		if ( parm7 == 0 )
		blend gl_one, gl_one_minus_src_color
			map models/monsters/mutate/fluidtube_add
			scroll (0.1 * sintable [time * 0.1]), time * 0.3
			rgb (random [time * 2])
	}
	{
		if ( parm7 == 0 )
		blend gl_one, gl_one_minus_src_color
			map models/monsters/mutate/fluidtube_add2
			scroll 0, bloodflow[time * 0.25] - time * 0.3
			centerscale 3, 1
			rgb (jitter10 [time * 2])

	}
	{
		if ( parm7 == 0 )
		blend gl_one, gl_one_minus_src_color
			map models/monsters/mutate/fluidtube_add3
			scroll (0.2 * sintable[time * 0.2]), 2 * bloodflow[time * 0.25] - time * 0.3
			rgb (jitter2 [time * 1])
			centerscale 3, 1
	}
	{
		if parm7
		blend diffusemap
		map	_black
	}
	{ // Scope view texture
		blend add
		map textures/sfx/monsterSniperView
		texgen screen
		scroll 0.5, 0.5
		scale 0.5, 0.5
		clamp
		glowStage
		scopeView	// Only visible in the rifle scope view
	}
}
