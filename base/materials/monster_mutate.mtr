
//
//
// a yuppie at the second stage of a mutate virus
//
models/monsters/mutate/alienhead
{
	noShadows
//	noselfshadow
	unsmoothedtangents
	matter_flesh
	forceOverlays

	renderbump -size 512 512 -aa 1 -colormap models/monsters/mutate/alienhead_local models/monsters/mutate/mutatehi.lwo

	diffusemap models/monsters/mutate/alienhead
//	bumpmap	addnormals(models/monsters/mutate/alienhead_local, heightmap(models/monsters/mutate/alienhead_h, 7 ) )
//	bumpmap	models/monsters/mutate/alienhead_local
	{
		blend bumpmap
		map addnormals(models/monsters/mutate/alienhead_local, heightmap(models/monsters/mutate/alienhead_h, 7 ) )
		highres
	}
	specularmap	models/monsters/mutate/alienhead_s

	{	// Burning flesh effect - burns away the diffuse map
		if	parm7			// only when dead

		// make a burned away alpha test for the normal skin
		blend	gl_zero, gl_one			// don't draw anything

		map models/monsters/mutate/alienhead_dis	// replace this with a monster-specific texture
		alphaTest 0.05 + 0.2 * (time - parm7)
	}
	{	// burning corpse effect - blueish effect that trails the burnout
		if parm7			// only when dead

		// draw the fire burn at a negative polygonOffset, so it is behind the other stages
		privatePolygonOffset	-1		// stage-only polygon offset
        blend add
        blend gl_one, gl_zero

		map models/monsters/mutate/alienhead_dis	// replace this with a monster-specific texture
		alphaTest 0.2 * (time - parm7)
		glowStage
	}

	{ // Scope view texture
		blend add
		map textures/sfx/monsterSniperView
		texgen screen
		scroll 0.5, 0.5
		scale 0.5, 0.5
		clamp
		glowStage
		scopeView	// Only visible in the rifle scope view
	}
}

models/monsters/mutate/humanhead
{
	noShadows
//	noselfshadow
	unsmoothedtangents
	matter_flesh
	forceOverlays

	renderbump -size 512 512 -aa 1 -colormap models/monsters/mutate/humanhead_local models/monsters/mutate/mutatehi.lwo

//	diffusemap models/monsters/mutate/humanhead
	{
		shaderFallback3
		if ( parm7 == 0 )
		blend diffusemap
		map	models/monsters/mutate/humanhead
		alphaTest 0.5
	}
	{
		shaderFallback3
		if parm7
		blend diffusemap
		map	models/monsters/mutate/humanhead
	}
/*	bumpmap	addnormals(models/monsters/mutate/humanhead_local, heightmap(models/monsters/mutate/humanhead_h, 7 ) )
//	bumpmap	models/monsters/mutate/humanhead_local
	specularmap	models/monsters/mutate/humanhead_s
*/
	{
		shaderFallback3
		blend bumpmap
		map	addnormals(models/monsters/mutate/humanhead_local, heightmap(models/monsters/mutate/humanhead_h, 7 ) )
		highres
	}
	{
		shaderFallback3
		blend specularmap
		map	models/monsters/mutate/humanhead_s
	}
	{
		shaderLevel3
		blend shader
		program skin.vfp

		fragmentparm	0	1, 0, 0, 0		// mask channel
		// fragmentparm	1	0.0, 0.15, .1	// sss color, power
		fragmentparm	1	0.05, 0.5, 0.4	// sss color, power
		fragmentparm	2	0.5, 0.5		// wrap around
		fragmentparm	3	1.5				// rim
		fragmentparm	4	2.4, 2.4, 2.4, 35		// specular color, power
		fragmentparm	5	1.9, 1.9, 1.9, 10		// glancing specular color, power
		fragmentparm	6	0.15

		fragmentmap 1 highres addnormals(models/monsters/mutate/humanhead_local, heightmap(models/monsters/mutate/humanhead_h, 7 ) )
		fragmentmap 4 models/monsters/mutate/humanhead
		fragmentmap 5 models/monsters/mutate/humanhead_s
		fragmentmap 6 models/monsters/mutate/humanhead_s
		fragmentmap 7 _white
	}

	{	// Burning flesh effect - burns away the diffuse map
		if	parm7			// only when dead

		// make a burned away alpha test for the normal skin
		blend	gl_zero, gl_one			// don't draw anything

		map models/monsters/mutate/humanhead_dis	// replace this with a monster-specific texture
		alphaTest 0.05 + 0.2 * (time - parm7)
	}
	{	// burning corpse effect - blueish effect that trails the burnout
		if parm7			// only when dead

		// draw the fire burn at a negative polygonOffset, so it is behind the other stages
		privatePolygonOffset	-1		// stage-only polygon offset
        blend add
        blend gl_one, gl_zero

		map models/monsters/mutate/humanhead_dis	// replace this with a monster-specific texture
		alphaTest 0.2 * (time - parm7)
		glowStage
	}

	{ // Scope view texture
		blend add
		map textures/sfx/monsterSniperView
		texgen screen
		scroll 0.5, 0.5
		scale 0.5, 0.5
		clamp
		glowStage
		scopeView	// Only visible in the rifle scope view
	}
}

