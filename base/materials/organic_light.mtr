{
	qer_editorimage 	textures/organic_light/orglightpatch4b_d
	matter_metal

	bumpmap 		addnormals (textures/organic_light/orglightpatch4b_local,
				heightmap (textures/organic_light/orglightpatch4b_h, 6))
	diffusemap 		textures/organic_light/orglightpatch4b_d
	specularmap		textures/organic_light/orglightpatch4b_s
	{
		blend		add
		map			textures/organic_light/orglightpatch4b_add
		color		( parm0 * parm5 ) + (( 1 - parm5 ) * parm0  * ((( biopulse1 [ parm3 + ( time * parm6 ) ] +1) * 0.5 ))),
				( parm1 * parm5 ) + (( 1 - parm5 ) * parm1  * ((( biopulse1 [ parm3 + ( time * parm6 ) ] +1) * 0.5 ))),
				( parm2 * parm5 ) + (( 1 - parm5 ) * parm2  * ((( biopulse1 [ parm3 + ( time * parm6 ) ] +1) * 0.5 ))),
				1.0
	}
}

