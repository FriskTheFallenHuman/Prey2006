textures/organic_floor/sphere_husk
{
	qer_editorimage		textures/dreamworld/la_darkrock_d
	matter_flesh
	bumpmap			textures/organic_floor/sphere_husk_local
	diffusemap		textures/organic_floor/sphere_husk_d
	specularmap		textures/organic_floor/sphere_husk_s
}

textures/organic_floor/bonefloor_vp
{
	matter_flesh
	qer_editorimage textures/feedingtower/bone3_d
	{
		blend	bumpmap
		map heightmap(textures/feedingtower/bone3_h, 1)
	}
	{
		blend diffusemap
		map		textures/feedingtower/bone3_d
	}
	{
		blend specularmap
		map		textures/feedingtower/bone3_s
	}
}



textures/organic_floor/circwall1_floora
{

//plain heightmap of circuitwall1, just the organic skin

	qer_editorimage 	textures/organic_floor/circwall1_floora_d
	matter_metal

	bumpmap 		heightmap (textures/organic_floor/circwall1_floora_h, 4)
	diffusemap		textures/organic_floor/circwall1_floora_d
	specularmap		textures/organic_floor/circwall1_floora_s
}

textures/organic_floor/circwall1_floora2
{

//plain heightmap of circuitwall1, more circuitry

	qer_editorimage 	textures/organic_floor/circwall1_floora_d2
	matter_metal

	bumpmap 		heightmap (textures/organic_wall/circuitwall1_h, 4)
	diffusemap		textures/organic_floor/circwall1_floora_d2
	specularmap		textures/organic_floor/circwall1_floora_s2
}

textures/organic_floor/circwall1_floora2_add
{

//plain heightmap of circuitwall1, more circuitry

	qer_editorimage 	textures/organic_floor/circwall1_floora_d2
	matter_metal

	bumpmap 		heightmap (textures/organic_wall/circuitwall1_h, 4)
	diffusemap		textures/organic_floor/circwall1_floora_d2
	specularmap		textures/organic_floor/circwall1_floora_s2
	{
		blend add
		map textures/organic_wall/circuitwall1_add
		rgb (jitter1 [time * 2]) - 0.1
	}
}

textures/organic_floor/circwall1_flooramove
{

//plain heightmap of circuitwall1 with moving specmap

	qer_editorimage 	textures/organic_floor/circwall1_floora_d
	matter_metal

	bumpmap 		heightmap (textures/organic_floor/circwall1_floora_h, 4)
	diffusemap		textures/organic_floor/circwall1_floora_d
	{
		blend 	specularmap
		map	textures/organic_wall/chitinwall1_s4
		scroll  (0.05 * sintable [time* 0.05]), time * -0.02
	}
}

