textures/organic_floor/circwall1_floore_add
{

//plain heightmap of circuitwall1, with hexagonal circuits and rounded bevel, plus
//lights on both sides

	qer_editorimage 	textures/organic_floor/circwall1_floore_d
	matter_metal

	bumpmap 		addnormals (textures/organic_floor/circwall1_floore_local,
				heightmap (textures/organic_floor/circwall1_floore_h, 4))
