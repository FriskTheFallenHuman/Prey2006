textures/tech_light/techlight_041_add_clean_shuttle
{
	qer_editorimage 	textures/tech_light/techlight_041_clean_proxy
	matter_metal

	bumpmap 		addnormals (textures/tech_light/techlight_041_local, heightmap (textures/tech_light/techlight_041_clean_h, 6))

	diffusemap	 	textures/tech_light/techlight_041_clean_d
	specularmap		textures/tech_light/techlight_041_clean_s
	{
		blend add
		map textures/tech_light/techlight_041_clean_add
		color		1*parm0, 1*parm1, 1*parm2, 1.0
	}
	{	// Shuttle view stage
                 blend add
		map textures/decals/shuttle_flicker_02_blue
                texgen screen
		 rgb (fade_pulse [time * .4] * 0.7)

                 scroll time * 10, time * 10.1 * ( parm7 + 10 )
                shuttleView
		glowstage
	}

                {	// Shuttle view stage

		maskalpha
		blend	gl_dst_alpha, gl_one
		map textures/decals/shuttle_flicker
		texgen screen
                 rgb (fade_pulse [time * .4] * 0.5)
                scale ( forcefieldflicker[ time * 1.0 ] * .01 ) , 0.5
		scroll time * 10, time * 10.1 * ( parm7 + 10 )
		shuttleView
		glowstage
	}
}
