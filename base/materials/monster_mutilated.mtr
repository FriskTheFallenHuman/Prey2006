models/monsters/mutilated/mutilated_head_frozen
{
	unsmoothedtangents
	matter_stone
	translucent
	collision
	noshadows
	noOverlays
	{
		shaderLevel1

		program			refraction.vfp

		vertexparm		1       3.5*fadeIce[1+.3*(parm7-time)]			// magnitude of the distortion

		fragmentparm	0		1-.1*fadeIce[1+.3*(parm7-time)], 1-.1*fadeIce[1+.3*(parm7-time)], 1-.04*fadeIce[1+.3*(parm7-time)], 1	// refraction color
		fragmentparm	1		.8*fadeIce[1+.3*(parm7-time)], .8*fadeIce[1+.3*(parm7-time)], .9*fadeIce[1+.3*(parm7-time)], 1		// reflection color
		fragmentparm	2		2*fadeIce[1+.3*(parm7-time)]

		fragmentmap		1		highres addnormals(models/monsters/mutilated/mutilated_head_local, heightmap(models/monsters/mutilated/mutilated_head_h, 7 ) )
		fragmentmap		2       _currentrender
	}
//	bumpmap	addnormals(models/monsters/mutilated/mutilated_head_local, heightmap(models/monsters/mutilated/mutilated_head_h, 7 ) )
	{
		blend bumpmap
		map addnormals(models/monsters/mutilated/mutilated_head_local, heightmap(models/monsters/mutilated/mutilated_head_h, 7 ) )
		highres
	}
	{
		blend blend
		map _white
		rgb .72
		alpha .35*fadeIce[1+.3*(parm7-time)]
	}
	{
		blend diffusemap
		map _white
		color .34*fadeIce[1+.3*(parm7-time)], .34*fadeIce[1+.3*(parm7-time)], .36*fadeIce[1+.3*(parm7-time)], 1
	}
}

