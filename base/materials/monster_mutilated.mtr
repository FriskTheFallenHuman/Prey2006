models/monsters/mutilated/mutilated_dome
{
//	noselfshadow
	glass_macro
	forceOverlays
	noshadows

	renderbump -size 1024 1024 -trace 0.03 -colormap models/monsters/mutilated/mutilated_local models/monsters/mutilated/mutilatedhi.lwo
//	diffusemap models/monsters/mutilated/mutilated
	{
		if parm7
		blend diffusemap
		map _BLACK
	}
	{
		if parm7 == 0
		blend gl_dst_color, gl_src_color
		map models/monsters/mutilated/mutilated
	}
//	bumpmap	addnormals(models/monsters/mutilated/mutilated_local, heightmap(models/monsters/mutilated/mutilated_h, 7 ) )
//	bumpmap	models/monsters/mutilated/mutilated_local
	{
		blend bumpmap
		map addnormals(models/monsters/mutilated/mutilated_local, heightmap(models/monsters/mutilated/mutilated_h, 7 ) )
		highres
	}
	{
		if parm7
		blend specularmap
		map models/monsters/mutilated/mutilated_s
		rgb 0
	}
	{
		if parm7 == 0
		blend specularmap
		map models/monsters/mutilated/mutilated_s
		rgb .2
	}
//	specularmap models/monsters/mutilated/mutilated_s
	{
		if parm7
		blend add
		cubemap	env/gs3
		texgen reflect
		rgb	0
	}
	{
		if parm7 == 0
		blend add
		cubemap	env/gs3
		texgen reflect
		rgb	0.2
	}
}

