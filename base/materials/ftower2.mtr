textures/feedingtower/ftower_trim_pipe6_dissolve
{
	qer_editorimage textures/feedingtower/fttrimpipe6
	matter_pipe
	bumpmap
		heightmap( textures/feedingtower/fttrimpipe6_b, 1.5 )
	specularmap	textures/feedingtower/fttrimpipe6_spec
	diffusemap textures/feedingtower/fttrimpipe6

	//global alphatest stages
	{
		if ( parm5 == 0 ) //fade in
		blend none
		map textures/sfx/feeding_ramp_fx_256
		alphaTest 1 - ( time + parm4 ) * 2
	}
	{
		if ( parm5 > 0 ) //fade out
		blend none
		map textures/sfx/feeding_ramp_fx_256
		alphaTest ( time + parm4 ) * 2
	}

	//additive fx stage
	{
		if ( parm5 == 0 ) //fade in
		privatepolygonoffset	-1
		blend add
		map textures/sfx/feeding_ramp_fx_256
		alphaTest ( 1 - ( time + parm4 ) * 2 ) - 0.075
		rgb 0.8
		glowStage
	}
	{
		if ( parm5 > 0 ) //fade out
		privatepolygonoffset	-1
		blend add
		map textures/sfx/feeding_ramp_fx_256
		alphaTest ( ( time + parm4 ) * 2 ) - 0.075
		rgb 0.8
		glowStage
	}
}

