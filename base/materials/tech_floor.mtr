	specularmap		textures/tech_floor/biotechfloor10b_s
}

textures/tech_floor/kf_biotechfloor10b_dissolve
{
	qer_editorimage 	textures/tech_floor/biotechfloor10b_proxy
	matter_metal
	bumpmap
	addnormals			(textures/tech_floor/biotechfloor10b_local,
	heightmap			(textures/tech_floor/biotechfloor10b_h, 0.7))
	diffusemap	 		textures/tech_floor/biotechfloor10b_d
	specularmap			textures/tech_floor/biotechfloor10b_s
	{
		if ( parm5 == 0 )
		blend none
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest 1 - ( time + parm4 ) * 0.5
	}
	{
		if ( parm5 > 0 ) //fade out
		blend none
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest ( time + parm4 ) * 0.5
	}
		{
		if ( parm5 == 0 ) //fade out
		privatepolygonoffset	-1
		blend add
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest 1 - ( time + parm4 ) * 0.5 + 0.1
		rgb 2
		glowStage
	}
	{
		if ( parm5 > 0 ) //fade out
		privatepolygonoffset	-1
		blend add
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest ( time + parm4 ) * 0.5 - 0.1
		rgb 2
		glowStage
	}
}

