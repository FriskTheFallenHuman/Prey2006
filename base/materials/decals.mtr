textures/decals/pipecap3_dissolve
{
	qer_editorimage textures/organic_wall/bio_organic_sw2_proxy
	matter_metal
	noshadows

	bumpmap			textures/decals/pipecap3_local
	diffusemap		textures/decals/pipecap3_d_dissolve
	specularmap		textures/decals/pipecap3_s

	//global alphatest stages
	{
		if ( parm5 == 0 )
		blend none
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest 1 - ( time + parm4 ) * 0.5
	}
	{
		if ( parm5 > 0 ) //fade out
		blend none
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest ( time + parm4 ) * 0.5
	}

	//additive fx stage
	{
		if ( parm5 == 0 ) //fade out
		privatepolygonoffset	-0.5
		blend add
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest 1 - ( time + parm4 ) * 0.5 + 0.1
		rgb 2
		glowStage
	}
	{
		if ( parm5 > 0 ) //fade out
		privatepolygonoffset	-0.5
		blend add
		map textures/sfx/keeperfortress_ramp_fx_256
		alphaTest ( time + parm4 ) * 0.5 - 0.1
		rgb 2
		glowStage
	}
}

