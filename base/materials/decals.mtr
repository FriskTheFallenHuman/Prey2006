textures/decals/painview
{
	translucent
	// FRAGMENT PROGRAM VERSION
	/*{
		if fragmentPrograms == 1
		blend blend
		alpha .0

		program			screeneffect.vfp

		fragmentmap		0       _currentrender
		fragmentmap		2		_white	//mask

		fragmentparm	0		.01*parm0, .09, 1-.45*parm0	//blur size, blur strength, blur lerp
		fragmentparm	1		1.2*parm0+(1-parm0), 0			//pre scale, pre bias
		fragmentparm	2		1-.75*parm0		//.65				//desateration
		fragmentparm	3		1+1.8*parm0, .15			//contrast, contrast center
		fragmentparm	4		.6, .1, .1, .5*parm0		//tint color
		fragmentparm	5		0.0, .0, .0	//post additive color
	}*/
	{
		blend blend

		program			screenprocess.vfp

		fragmentparm	0	1		// saturation
		fragmentparm	1	1, .5	// contrast, scale point
		fragmentparm	2	1	// multiply
		fragmentparm	3	0	// add/brightness

		fragmentmap		0       _currentrender
	}
}

