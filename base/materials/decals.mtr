textures/decals/superportal_wallburnout
{
	decal_macro
	qer_editorimage 	textures/decals/superwall_burn

	{ if ( parm4 == 0 )
		blend	add
		map		textures/decals/superwall_burn
		red		0
		green	0
		blue	0
	}

	{ if ( parm4 != 0 )
		blend	add
		map		textures/decals/superwall_burn
		red		parm0 * fade_on1 [ ( time + parm4 ) * 0.25 ]
		green	parm1 * fade_on1 [ ( time + parm4 ) * 0.25 ]
		blue	parm2 * fade_on1 [ ( time + parm4 ) * 0.25 ]
		glowstage
	}
}