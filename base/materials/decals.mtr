//----------------------------------------------------------------
// decals:  these are decorations applied to the levels over the top of other geometry,
//	so they are non-solid
//
// they should use modulation (blend gl_dst_color, gl_src_color) so they aren't fullbright
// id sometimes uses  (blend gl_dst_color, gl_zero) for these
//----------------------------------------------------------------

textures/decals/cracks
{
	qer_editorimage	textures/decals/cracks
	decal_macro

	// decalinfo <stayseconds> <fadeseconds> ( <start rgb> ) ( <end rgb> )

	decalinfo 1 2 (0.6 0.6 0.6 0.6) (0 0 0 0)
	{
		blend 	gl_zero,gl_one_minus_src_color
		map 	textures/decals/cracks
		vertexcolor
	}
}

textures/decals/hider_melt
{
	qer_editorimage	textures/decals/hider_melt
	decal_macro

	// decalinfo <stayseconds> <fadeseconds> ( <start rgb> ) ( <end rgb> )

	decalinfo 8 4 (0.4 0.4 0.4 0.4) (0 0 0 0)
	{
		blend 	add
		map 	textures/decals/hider_melt
		centerscale (pulsebeat [time / 80]),(pulsebeat [time / 80])
		rotate (time / 300)
		glowstage
		vertexcolor
		clamp
	}
}

