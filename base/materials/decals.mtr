textures/decals/frost
{
	qer_editorimage textures/decals/frost
	decal_alphatest_macro
	 {
                vertexProgram           heatHaze.vfp
                vertexParm              0       0, 0		// texture scrolling
                vertexParm              1       0.8				// magnitude of the distortion
                fragmentProgram			heatHaze.vfp
                fragmentMap             0       _currentRender
                fragmentMap             1       textures/decals/frost_local
     }
     {
		blend	add
	 	map		textures/decals/frost
	 	clamp
	}
}

