textures/decals/bloodsplat1
{
	decal_macro

	decalinfo 20 5 (1 1 1 1) (0 0 0 0)
	{
		blend 	gl_zero,gl_one_minus_src_color
		//map	textures/decals/gib_splat_i2
		map	textures/decals/bloodsplat1
		vertexcolor
	}
	{	// Scope view texture
      	blend		add
		map	textures/decals/bloodsplatscope
		clamp		// we don't want it to tile if the projection extends past the bounds
		vertexcolor	// oblique projections will be slightly faded down
		red 0.45
		green 0.2
		blue 0
		glowstage
		scopeView
	}
}

