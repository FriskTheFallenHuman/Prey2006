#define TEAMRED_ACTIVE_BACKCOLOR_STRING		"1 0 0 1"
#define TEAMRED_INACTIVE_BACKCOLOR_COMMA	1, 0, 0, 0.2
#define TEAMRED_INACTIVE_BACKCOLOR_STRING	"1 0 0 0.2"

