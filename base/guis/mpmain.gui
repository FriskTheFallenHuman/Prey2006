	onActivate {
		setFocus "Chat_Message" ;
		resetTime "Initialize" "0";
		set "MainTabs::activeTab" "0";

		if ( "gui::teamon" != 0 ) {			// Team play valid
			if ( "gui::team" == 0 ) {
				set "TeamRedRadio::noevents" "1";
				set "TeamRedRadio::backcolor" TEAMRED_ACTIVE_BACKCOLOR_STRING ;
				set "TeamBlueRadio::noevents" "0";
				set "TeamBlueRadio::backcolor" TEAMBLUE_INACTIVE_BACKCOLOR_STRING ;
			} else {
				set "TeamRedRadio::noevents" "0";
				set "TeamRedRadio::backcolor" TEAMRED_INACTIVE_BACKCOLOR_STRING ;
				set "TeamBlueRadio::noevents" "1";
				set "TeamBlueRadio::backcolor" TEAMBLUE_ACTIVE_BACKCOLOR_STRING ;
			}
		}
		else {
			set "TeamRedRadio::backcolor" TEAMRED_INACTIVE_BACKCOLOR_STRING ;
			set "TeamBlueRadio::backcolor" TEAMBLUE_INACTIVE_BACKCOLOR_STRING ;
			set "TeamRedRadio::noevents" "1";
			set "TeamBlueRadio::noevents" "1";
		}

		if ( "gui::callvote" == 0 ) {
			set "CallVoteUnavailable::text" "#str_20565" ;
		} else if ( "gui::callvote" == 1 ) {
			// Some votes are available, automatically enabled with noevents in the windowdef
			set "CallVoteUnavailable::text" "" ;
		} else if ( "gui::callvote" == 2 ) {
			set "CallVoteUnavailable::text" "#str_20566" ;
			set "MainTabs::activeTab" "2";
		}
	}

