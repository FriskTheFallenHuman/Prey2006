			tabDef Options_Audio {
				rect			0, 20, 570, 370
				backcolor		0, 0, 0, 0
				forecolor		TABTOP_FORECOLOR_COMMA
				hovercolor		TABTOP_HOVERCOLOR_COMMA
				activeColor		TABTOP_ACTIVECOLOR_COMMA
				hoverMatColor	TABTOP_HOVERMAT_COLOR_COMMA
				text			"#str_20310"
				seperatorLines	1, 1, 1, 1
				seperatorMargin	0

				onNamedEvent UpdateOptions {
					// Set initial state of our sound options based on the state variables
					// Called whenever options may have changed

					// Mixer Grayed based on mixeroption
					// Device Grayed unless openAL is ON
					// Speakers Grayed unless openAL is OFF
					// Reverse Grayed unless openAL is OFF
					// Driver Button visible if oldAudioDriver
					
					if ("gui::oldAudioDriver" != 0) {
						set "OA_SoundDriverNotice::visible" "1";
					}
					else {
						set "OA_SoundDriverNotice::visible" "0";
					}

					// If no eax capable devices are available, disable the mixer change
					if ("gui::mixeroption" != 0) {
						// Enable mixer choice
						set "OA_SoundSystemChoice::noevents" "0";
						set "OA_SoundSystemChoice::forecolor" EDITABLE_FORECOLOR_STRING;
					}
					else {
						// Disable mixer choice
						set "OA_SoundSystemChoice::noevents" "1" ;
						set "OA_SoundSystemChoice::forecolor" EDITABLE_FORECOLOR_GRAYED_STRING ;
					}

					if ("gui::openAL" != 0) {		// OpenAL is ON
						// Enable device choice
						set "OA_DeviceChoice::noevents" "0" ;
						set "OA_DeviceChoice::forecolor" EDITABLE_FORECOLOR_STRING ;

						// Disable speaker choice
						set "OA_SpeakerSetupChoice::noevents" "1" ;
						set "OA_SpeakerSetupChoice::forecolor" EDITABLE_FORECOLOR_GRAYED_STRING ;

						// Disable Reverse choice
						set "OA_ReverseChannelsChoice::noevents" "1" ;
						set "OA_ReverseChannelsChoice::forecolor" EDITABLE_FORECOLOR_GRAYED_STRING ;
					}
					else {							// OpenAL is OFF
						// Disable device choice
						set "OA_DeviceChoice::noevents" "1" ;
						set "OA_DeviceChoice::forecolor" EDITABLE_FORECOLOR_GRAYED_STRING ;

						// Enable Speaker choice
						set "OA_SpeakerSetupChoice::noevents" "0" ;
						set "OA_SpeakerSetupChoice::forecolor" EDITABLE_FORECOLOR_STRING ;

						// Enable Reverse choice
						set "OA_ReverseChannelsChoice::noevents" "0" ;
						set "OA_ReverseChannelsChoice::forecolor" EDITABLE_FORECOLOR_STRING ;
					}

					// EAX Grayed unless openAL is on and device supports EAX
					if ( ("gui::openAL" != 0) && ("gui::supportsEax" != 0) ) {
						set "OA_EAXChoice::noevents" "0" ;
						set "OA_EAXChoice::forecolor" EDITABLE_FORECOLOR_STRING ;
					}
					else {
						set "OA_EAXChoice::noevents" "1" ;
						set "OA_EAXChoice::forecolor" EDITABLE_FORECOLOR_GRAYED_STRING ;
					}
				}

				onTabActivate {
					set "cmd" "play guisounds_menu_tabflip" ;
				}

