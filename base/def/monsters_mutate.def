export all {
	options		-scale 1 -prefix mutate_ -keep Waist FX_canisterdrip1 FX_bellyventA FX_bellyventB FX_canisterdrip fx_canister fx_wormMouth -rename joint2 Rshldr


	mesh		models/monsters/mutate/mutate.ma -destdir models/monsters/mutate
    
   
	addoptions	-destdir models/monsters/mutate/anim
	
//	anim		models/monsters/mutate/anim/birth.ma
//	anim		models/monsters/mutate/anim/birthIdle.ma

	 addoptions  -prefix mutate_
	 anim		models/monsters/mutate/anim/initial.ma
	 anim		models/monsters/mutate/anim/idle.ma
	 anim		models/monsters/mutate/anim/run.ma
	 anim		models/monsters/mutate/anim/run2.ma
	 anim		models/monsters/mutate/anim/walkback.ma
	 anim		models/monsters/mutate/anim/start_run.ma
	 anim		models/monsters/mutate/anim/attack.ma
	 anim		models/monsters/mutate/anim/runattack1.ma
	 anim		models/monsters/mutate/anim/runattack2.ma
	 anim		models/monsters/mutate/anim/attack_overhead.ma
	 anim		models/monsters/mutate/anim/attack_overhead_stuck.ma
	 anim		models/monsters/mutate/anim/attack_uppercut_fast.ma
	 anim		models/monsters/mutate/anim/fat_attack.ma
	 anim		models/monsters/mutate/anim/walk.ma
	 anim		models/monsters/mutate/anim/start_walk.ma
	 anim		models/monsters/mutate/anim/pain_head.ma
	 anim		models/monsters/mutate/anim/pain_rightside.ma
	 anim		models/monsters/mutate/anim/pain_leftside.ma
	 anim		models/monsters/mutate/anim/run_pain1.ma
	 anim		models/monsters/mutate/anim/evade_left.ma
	 anim		models/monsters/mutate/anim/evade_right.ma
	 anim		models/monsters/mutate/anim/strafe_left.ma
	 anim		models/monsters/mutate/anim/strafe_right.ma
	 anim		models/monsters/mutate/anim/bed_idle.ma
	 anim		models/monsters/mutate/anim/bed_wake.ma
	 anim		models/monsters/mutate/anim/360attack.ma
	 anim		models/monsters/mutate/anim/attack_LRswipe.ma
	 anim		models/monsters/mutate/anim/surprised.ma
	 anim		models/monsters/mutate/anim/breath.ma
	 anim		models/monsters/mutate/anim/turn.ma
	 anim		models/monsters/mutate/anim/mutate_PR1.ma
}
model model_monster_mutate {
	mesh							models/monsters/mutate/mutate.md5mesh
	channel torso					(*wormNeck_1 *Lback_3)
	channel legs					(*origin -*back_2 *joint35 Rback_3 )

	anim af_pose					models/monsters/mutate/anim/initial.md5anim
	anim ik_pose					models/monsters/mutate/anim/initial.md5anim
	anim initial					models/monsters/mutate/anim/initial.md5anim
	anim mutate_pr1					models/monsters/mutate/anim/mutate_PR1.md5anim
	anim run2					models/monsters/mutate/anim/run2.md5anim	{
	
		frame 1						sound snd_footstep
		frame 20					sound snd_footstep
		frame 34					sound snd_footstep
	
	}
	anim runattack2					models/monsters/mutate/anim/runattack2.md5anim	{
		frame 2						sound snd_footstep
		frame 21					sound snd_footstep
		frame 24						sound snd_footstep
	}
	anim breath						models/monsters/mutate/anim/breath.md5anim {
		frame 1						event_args spawnFXAlongBone fx_drool fx_wormMouth
	}
	anim turn						models/monsters/mutate/anim/turn.md5anim {
		frame 1							disableWalkIK
		frame 13						sound snd_footstep
		frame 17						sound snd_footstep
		frame 17						enableWalkIK
	}
	anim portal						models/monsters/mutate/anim/run.md5anim {
		frame 1							disableWalkIK
		frame 8							sound_body snd_sight_monster
		frame 9							sound_voice snd_sight_human
		frame 17						enableWalkIK
		frame 19						sound snd_footstep
		frame 34						sound snd_footstep
		frame 37						sound snd_footstep
	}  
	anim idle						models/monsters/mutate/anim/idle.md5anim {
		ai_no_turn
		frame 1						event_args spawnFXAlongBone fx_drool fx_wormMouth
	}
	anim evade_left					models/monsters/mutate/anim/evade_left.md5anim {
		frame 1							sound snd_footstep
		frame 2							disableWalkIK
		frame 11						sound snd_footstep
		frame 16						sound snd_footstep
		frame 14						enableWalkIK
	}
	anim evade_right				models/monsters/mutate/anim/evade_right.md5anim {
		frame 1							sound snd_footstep
		frame 2							disableWalkIK
		frame 11						sound snd_footstep
		frame 16						sound snd_footstep
		frame 14						enableWalkIK
	}
	anim run						models/monsters/mutate/anim/run.md5anim {
		frame 1							disableWalkIK
		frame 17						enableWalkIK
		frame 19						sound snd_footstep
		frame 34						sound snd_footstep
		frame 37						sound snd_footstep
		
	}  
	anim walkback					models/monsters/mutate/anim/walkback.md5anim {
		frame 4							disableWalkIK
		frame 12					sound snd_footstep
		frame 15						enableWalkIK
		frame 17					sound snd_footstep
	}
	anim spinattack2				models/monsters/mutate/anim/runattack1.md5anim {
		ai_no_turn
		frame 1							disableWalkIK
		frame 1							sound_body snd_meleeattack_monster
		frame 10						melee melee_mutate_quick
		frame 21						sound snd_footstep
		frame 23						enableWalkIK
		frame 23.1						sound snd_footstep
	}
	anim spinattack1				models/monsters/mutate/anim/360attack.md5anim {
		ai_no_turn
		frame 1							sound_body snd_meleeattack_monster
		frame 5							disableWalkIK
		frame 6							melee melee_mutate_quick
		frame 22						enableWalkIK
		frame 23						melee melee_mutate_spin	
		frame 23.1						sound snd_footstep
		frame 24						sound snd_footstep
		frame 30						melee melee_mutate_quick
	}
	anim fatswing					models/monsters/mutate/anim/fat_attack.md5anim {
		frame 3							sound_voice snd_meleeattack_human
		frame 4							melee melee_mutate_fat
	}
	anim attack_overhead_stuck						models/monsters/mutate/anim/attack_overhead_stuck.md5anim {
		ai_no_turn
		frame 1							sound_body snd_meleeattack_monster
		frame 19						melee melee_mutate_stuck
		frame 17						sound snd_meleeimpact
		frame 17						event_args spawnFXAlongBone fx_spike joint75
		frame 38						sound_voice snd_sight_human
	}
	anim attack_uppercut_fast						models/monsters/mutate/anim/attack_uppercut_fast.md5anim {
		frame 15							sound_body snd_meleeattack_monster
		frame 17							melee melee_mutate_quick
	}
	anim attack_LRswipe					models/monsters/mutate/anim/attack_LRswipe.md5anim {
		frame 7								sound_body snd_meleeattack_monster
		frame 7							melee melee_mutate_quick
		frame 16							melee melee_mutate_quick
		frame 17							sound_voice snd_meleeattack_monster
	}
	anim walk						models/monsters/mutate/anim/walk.md5anim {
		frame 2							rightfoot
		frame 13						leftfoot
	}		
	anim pain_head					models/monsters/mutate/anim/pain_head.md5anim	{
	
		frame 1					sound_voice snd_pain_human
		frame 2					sound_body snd_pain_monster
		
	}
	anim pain_rightSide				models/monsters/mutate/anim/pain_rightside.md5anim	{
	
		frame 1					sound_body snd_pain_monster
	
	}
	anim pain           			models/monsters/mutate/anim/pain_rightside.md5anim	{	
	
		frame 1					sound_body snd_pain_monster
	
	}
	anim run_pain1           			models/monsters/mutate/anim/run_pain1.md5anim	{	
	
		frame 1					sound_body snd_pain_monster
	
	}
	anim pain_leftSide				models/monsters/mutate/anim/pain_leftside.md5anim	{
	
		frame 1					sound_voice snd_pain_human
	
	}
	anim strafe_left					models/monsters/mutate/anim/strafe_left.md5anim {
		frame 3							disableWalkIK
		frame 11						enableWalkIK
		frame 12						leftfoot
		frame 14						rightfoot
	}
	anim strafe_right				models/monsters/mutate/anim/strafe_right.md5anim {
		frame 3							disableWalkIK
		frame 11						enableWalkIK
		frame 12						leftfoot
		frame 13						rightfoot
	}
//Bed Anims FIXME: replace with actual anims [ mf 12/1/04 ]

	anim bed_idle					models/monsters/mutate/anim/bed_idle.md5anim
	anim bed_wake					models/monsters/mutate/anim/bed_wake.md5anim	{
	
		frame 30				sound_body snd_sight_monster
		frame 39				sound_voice snd_sight_human
		
	}
