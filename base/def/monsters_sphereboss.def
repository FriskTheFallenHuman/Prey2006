export all {
	options		-scale 1.0	-prefix boss_	-keep Boss ALL waistLook
	mesh		models/monsters/sphere_boss/sphereboss.ma	-destdir models/monsters/sphere_boss

	addoptions	-destdir models/monsters/sphere_boss/anim
	anim		models/monsters/sphere_boss/anim/idle.ma
	anim		models/monsters/sphere_boss/anim/forward.ma
	anim		models/monsters/sphere_boss/anim/backward.ma
	anim		models/monsters/sphere_boss/anim/strafe_left.ma
	anim		models/monsters/sphere_boss/anim/strafe_right.ma
	anim		models/monsters/sphere_boss/anim/swoop_attack.ma
	anim		models/monsters/sphere_boss/anim/rapid_fire.ma
	anim		models/monsters/sphere_boss/anim/single_fire.ma
	anim		models/monsters/sphere_boss/anim/stream_fire.ma
	anim		models/monsters/sphere_boss/anim/pain.ma
	anim		models/monsters/sphere_boss/anim/death.ma
}

model model_monster_sphereboss_stage2 {
	inherit model_monster_sphereboss
	anim burst_attack			models/monsters/sphere_boss/anim/forward.md5anim {
		frame 1						launch_missile b3 projectile_sphere_slow
	}
	anim range_attack			models/monsters/sphere_boss/anim/rapid_fire.md5anim {
		frame 1						sound snd_burst 
		frame 1						launch_missile b3 projectile_sphere_burst
		frame 6						sound snd_burst 
		frame 6						launch_missile b3 projectile_sphere_burst
	}
}

model model_monster_sphereboss {	
	mesh						models/monsters/sphere_boss/sphereboss.md5mesh
	channel torso				(*origin)
	channel legs				()
	
	anim shoot					models/monsters/sphere_boss/anim/forward.md5anim {
		frame 1						launch_missile b3 projectile_fire_column_side
		frame 1						sound snd_fireattack
	}
	anim fly					models/monsters/sphere_boss/anim/forward.md5anim
	anim idle		        	models/monsters/sphere_boss/anim/idle.md5anim
	anim charge		        	models/monsters/sphere_boss/anim/swoop_attack.md5anim
	anim pain					models/monsters/sphere_boss/anim/pain.md5anim
	anim predeath				models/monsters/sphere_boss/anim/idle.md5anim {
		frame 50					event_args spawnFXAlongBone fx_dead Boss noremovewhenunbound
		frame 50					sound snd_rumble
	}
	anim prefire				models/monsters/sphere_boss/anim/idle.md5anim {
		frame 1					event_args spawnFXAlongBone fx_attack Boss noremovewhenunbound
	}
	anim death					models/monsters/sphere_boss/anim/death.md5anim {
		frame 23						sound snd_explode
	}
	anim burst_attack			models/monsters/sphere_boss/anim/forward.md5anim {
		frame 1						launch_missile b3 projectile_sphere_slow
		frame 2						launch_missile b3 projectile_sphere_slow
		frame 3						launch_missile b3 projectile_sphere_slow
	}
	anim range_attack			models/monsters/sphere_boss/anim/rapid_fire.md5anim {
		frame 1						sound snd_burst 
		frame 1						launch_missile b3 projectile_sphere_burst
		frame 3						sound snd_burst 
		frame 3						launch_missile b3 projectile_sphere_burst
		frame 6						sound snd_burst 
		frame 6						launch_missile b3 projectile_sphere_burst
		frame 9						sound snd_burst 
		frame 9						launch_missile b3 projectile_sphere_burst
		frame 12					sound snd_burst 
		frame 12						launch_missile b3 projectile_sphere_burst
	}
}
