export all {
	options		-scale 1 -prefix Jane_ -keep waist b1 hip b2 Rupleg Lupleg b3 Rknee Lknee neck Rclav Lclav Rankle Lankle head Rshldr Lshldr Rtoe_r Ltoe_r Mbrow Rbrow Ruplid Rlolid Rcheek M_rcorner M_urlip M_ulip jaw M_ullip M_lcorner Lcheek Llolid Luplid Lbrow Leye Reye Rpigtail1 Lpigtail1 Ruparm Luparm M_blip M_brlip M_bllip Lfocus Rfocus Rpigtail2 Lpigtail2 Relbow Lelbow Rpigtail3 Lpigtail3 Rwrist Lwrist R_indx1 R_mddl1 R_rng1 R_pnky1 R_thmb1 Rattach_hand L_indx1 L_mddl1 L_rng1 L_pnky1 L_thmb1 Lattach_hand R_indx2 R_mddl2 R_rng2 R_pnky2 R_thmb2 L_indx2 L_mddl2 L_rng2 L_pnky2 L_thmb2 R_indx3 R_mddl3 R_rng3 R_pnky3 R_thmb3 L_indx3 L_mddl3 L_rng3 L_pnky3 L_thmb3
	
	addoptions -keepmesh Peter_mesh

	mesh		models/monsters/children/peter.ma -destdir models/monsters/children
}

model model_monster_possessed_boy {
	inherit					model_monster_possessed_girl
	
	mesh					models/monsters/children/peter.md5mesh
	channel torso			(*waist -*hip)
	channel legs			(*hip origin)
	
	anim trapped 			models/monsters/children/anim/peter_trapped.md5anim {
		frame 110			trigger girl2behindyou
		frame 155			trigger girl2onback
		frame 280			trigger girl2panting
		frame 310			trigger girlspikeaccent
		frame 320			trigger girl2help2
		frame 355			call map_feedingtowerc::KidPossessedSpike_Thread
	}
	anim trap_Loop			models/monsters/children/anim/peter_loop.md5anim	
	anim af_pose			models/monsters/children/anim/initial.md5anim
	anim ik_pose			models/monsters/children/anim/initial.md5anim
	anim initial			models/monsters/children/anim/initial.md5anim
	anim idle				models/monsters/children/anim/idle.md5anim
	anim run				models/monsters/children/anim/run.md5anim
	anim possessed_death	models/monsters/children/anim/explode.md5anim {
		frame 19				event kill
	}
}

entityDef monster_possessed_boy {
	"inherit"				"monster_possessed_girl"
	"model"					"model_monster_possessed_boy"
	"skin"					"skins/monster/possessedboy"
}

